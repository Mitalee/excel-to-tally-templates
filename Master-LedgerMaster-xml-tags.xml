	<COLUMNNAME.LIST>
		<COLUMNNAME>NAME</COLUMNNAME>
		<COLUMNNAME>ID</COLUMNNAME>
	</COLUMNNAME.LIST>
	
	<MASTER TYPE="GROUP">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="D"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="E"/>
	</MASTER>
	
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="A"/>
			<NAME COLUMNREFERENCE="C"/>
