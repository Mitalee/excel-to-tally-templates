<!-- These XML tags are added to Excel sheet at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that this (i.e Column as mentioned above) is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>NAME</COLUMNNAME>
		<COLUMNNAME>ID</COLUMNNAME>
	</COLUMNNAME.LIST>
	<MASTER TYPE="GROUP">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="E"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="F"/>
	</MASTER>
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="B"/>
			<NAME SCROLL="YES" COLUMNREFERENCE="D"/>
		</NAME.LIST>
		<OLDNAME COLUMNREFERENCE="C"/>
		<ADDITIONALNAME COLUMNREFERENCE="B"/>
		<PARENT COLUMNREFERENCE="E"/>
		<OPENINGBALANCE FORMULA="=Round(G#+H#,2)*if(G#&gt;0,-1,1)"/>

		<ISBILLWISEON COLUMNREFERENCE="I"/>
		<BILLCREDITPERIOD COLUMNREFERENCE="J"/>
		<BILLALLOCATIONS.LIST SKIP="=len(trim(K#))=0" SCROLL="YES">
			<NAME COLUMNREFERENCE="K"/>
			<BILLDATE COLUMNREFERENCE="L"/>
			<BILLCREDITPERIOD COLUMNREFERENCE="M"/>
			<OPENINGBALANCE FORMULA="=Round(N#+O#,2)*if(N#&gt;0,-1,1)"/>
		</BILLALLOCATIONS.LIST>
		<ADDRESS.LIST>
			<ADDRESS COLUMNREFERENCE="W"/>
			<ADDRESS COLUMNREFERENCE="X"/>
			<ADDRESS COLUMNREFERENCE="Y"/>
			<ADDRESS COLUMNREFERENCE="Z"/>
		</ADDRESS.LIST>
		<LEDGERCONTACT COLUMNREFERENCE="P"/>
		<LEDGERPHONE COLUMNREFERENCE="Q"/>
		<LEDGERMOBILE COLUMNREFERENCE="R"/>
		<LEDGERFAX COLUMNREFERENCE="S"/>
		<EMAIL COLUMNREFERENCE="T"/>
		<STATENAME COLUMNREFERENCE="U"/>
		<!--<LEDSTATENAME COLUMNREFERENCE="U"/>-->
		<PINCODE COLUMNREFERENCE="V"/>
		<INCOMETAXNUMBER COLUMNREFERENCE="AA"/>
		<REGISTRATIONNUMBER COLUMNREFERENCE="AB"/>
		<VATTINNUMBER COLUMNREFERENCE="AC"/>
		<SALESTAXNUMBER COLUMNREFERENCE="AC"/>
		<INTERSTATESTNUMBER COLUMNREFERENCE="AD"/>
		<ISTDSAPPLICABLE COLUMNREFERENCE="AE"/>
		<ISSTXPARTY COLUMNREFERENCE="AF"/>
		<PAYMENTDETAILS.LIST>
			<IFSCODE COLUMNREFERENCE="AG"/>
			<BANKNAME COLUMNREFERENCE="AI"/>
			<ACCOUNTNUMBER COLUMNREFERENCE="AH"/>
			<PAYMENTFAVOURING COLUMNREFERENCE="B"/>
			<TRANSACTIONNAME COLUMNREFERENCE="B"/>
			<SETASDEFAULT>Yes</SETASDEFAULT>
			<DEFAULTTRANSACTIONTYPE>Inter Bank Transfer</DEFAULTTRANSACTIONTYPE>
		</PAYMENTDETAILS.LIST>
		<AFFECTSSTOCK COLUMNREFERENCE="AJ"/>
		
		<EXCISEREGNO COLUMNREFERENCE="AK"/>
		<EXCISEREGISTRATIONDATE COLUMNREFERENCE="AL"/>
		<EXCISERANGE COLUMNREFERENCE="AM"/>
		<EXCISEDIVISION COLUMNREFERENCE="AN"/>
		<EXCISECOMMISSIONERATE COLUMNREFERENCE="AO"/>		
	</MASTER>
</XMLTAGS>

