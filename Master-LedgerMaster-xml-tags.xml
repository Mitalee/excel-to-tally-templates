		<OPENINGBALANCE COLUMNREFERENCE="F"/>
		<ADDRESS.LIST>
			<ADDRESS COLUMNREFERENCE="N"/>
			<ADDRESS COLUMNREFERENCE="O"/>
			<ADDRESS COLUMNREFERENCE="P"/>
		</ADDRESS.LIST>
		<LEDGERCONTACT COLUMNREFERENCE="G"/>
		<LEDGERPHONE COLUMNREFERENCE="H"/>
		<LEDGERMOBILE COLUMNREFERENCE="I"/>
		<LEDGERFAX COLUMNREFERENCE="J"/>
		<EMAIL COLUMNREFERENCE="K"/>
		<STATENAME COLUMNREFERENCE="L"/>
