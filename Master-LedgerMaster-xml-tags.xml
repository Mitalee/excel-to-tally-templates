	<COLUMNNAME.LIST>
		<COLUMNNAME>NAME</COLUMNNAME>
		<COLUMNNAME>ID</COLUMNNAME>
	</COLUMNNAME.LIST>
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="A"/>
			<NAME COLUMNREFERENCE="M"/>
		</NAME.LIST>
		<OLDNAME COLUMNREFERENCE="B"/>
		<ADDITIONALNAME COLUMNREFERENCE="A"/>
		<PARENT COLUMNREFERENCE="C"/>
		<OPENINGBALANCE COLUMNREFERENCE="D"/>
		<ADDRESS.LIST>
			<ADDRESS COLUMNREFERENCE="J"/>
			<ADDRESS COLUMNREFERENCE="K"/>
			<ADDRESS COLUMNREFERENCE="L"/>
		</ADDRESS.LIST>
		<LEDGERPHONE COLUMNREFERENCE="F"/>
		<LEDGERCONTACT COLUMNREFERENCE="E"/>
		<EMAIL COLUMNREFERENCE="G"/>
		<STATENAME COLUMNREFERENCE="H"/>
		<!--<LEDSTATENAME COLUMNREFERENCE="H"/>-->
		<PINCODE COLUMNREFERENCE="I"/>
		<INCOMETAXNUMBER COLUMNREFERENCE="N"/>
		<REGISTRATIONNUMBER COLUMNREFERENCE="O"/>
		<VATTINNUMBER COLUMNREFERENCE="P"/>
		<SALESTAXNUMBER COLUMNREFERENCE="P"/>
		<INTERSTATESTNUMBER COLUMNREFERENCE="Q"/>
		<ISTDSAPPLICABLE COLUMNREFERENCE="R"/>
		<ISSTXPARTY COLUMNREFERENCE="S"/>
		<PAYMENTDETAILS.LIST>
			<IFSCODE COLUMNREFERENCE="T"/>
			<BANKNAME COLUMNREFERENCE="V"/>
			<ACCOUNTNUMBER COLUMNREFERENCE="U"/>
			<PAYMENTFAVOURING COLUMNREFERENCE="A"/>
			<TRANSACTIONNAME COLUMNREFERENCE="A"/>
