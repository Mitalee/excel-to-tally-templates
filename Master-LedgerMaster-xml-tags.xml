<!-- These XML tags are added to Excel sheet at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that this (i.e Column as mentioned above) is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>NAME</COLUMNNAME>
		<COLUMNNAME>ID</COLUMNNAME>
	</COLUMNNAME.LIST>
	<MASTER TYPE="GROUP">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="E"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="F"/>
	</MASTER>
	<MASTER TYPE="CURRENCY">
		<ORIGINALNAME COLUMNREFERENCE="G"/>
		<EXPANDEDSYMBOL FORMULA="=G# &amp; &quot; Curr.&quot;"/>
		<MAILINGNAME FORMULA="=G# &amp; &quot; Curr.&quot;"/>
		<ASORIGINAL>Yes</ASORIGINAL>
		<HASSPACE>No</HASSPACE>
		<INMILLIONS>No</INMILLIONS>
		<DECIMALPLACES> 2</DECIMALPLACES>
	</MASTER>
	<MASTER TYPE="LEDGER">
		<NAME.LIST SKIP="=len(trim(D#))=0">
			<NAME COLUMNREFERENCE="B"/>
			<NAME SCROLL="YES" COLUMNREFERENCE="D"/>
		</NAME.LIST>
		<NAME SKIP="=Not(len(trim(D#))=0)" COLUMNREFERENCE="B"/>
		<OLDNAME COLUMNREFERENCE="C"/>
		<ADDITIONALNAME COLUMNREFERENCE="B"/>
		<PARENT COLUMNREFERENCE="E"/>
		<CURRENCYNAME COLUMNREFERENCE="G"/>
		<CREDITLIMIT COLUMNREFERENCE="L"/>
		<ISCREDITDAYSCHKON COLUMNREFERENCE="M"/>
		<OVERRIDECREDITLIMIT COLUMNREFERENCE="N"/>
		<OPENINGBALANCE SKIP="=And(len(trim(H#))=0,len(trim(I#))=0)" FORMULA="=Round(H#+I#,2)*if(H#&gt;0,-1,1)"/>
		<ISBILLWISEON COLUMNREFERENCE="J"/>
		<BILLCREDITPERIOD COLUMNREFERENCE="K"/>
		<BILLALLOCATIONS.LIST SKIP="=len(trim(O#))=0" SCROLL="YES">
			<NAME COLUMNREFERENCE="O"/>
			<BILLDATE COLUMNREFERENCE="P"/>
			<BILLCREDITPERIOD COLUMNREFERENCE="Q"/>
			<OPENINGBALANCE FORMULA="=Round(R#+S#,2)*if(R#&gt;0,-1,1)"/>
		</BILLALLOCATIONS.LIST>
		<ADDRESS.LIST SKIP="=And(len(trim(AB#))=0,len(trim(AC#))=0,len(trim(AD#))=0,len(trim(AE#))=0)">
			<ADDRESS COLUMNREFERENCE="AB"/>
			<ADDRESS COLUMNREFERENCE="AC"/>
			<ADDRESS COLUMNREFERENCE="AD"/>
			<ADDRESS COLUMNREFERENCE="AE"/>
		</ADDRESS.LIST>
		<LEDGERCONTACT COLUMNREFERENCE="T"/>
		<LEDGERPHONE COLUMNREFERENCE="U"/>
		<LEDGERMOBILE COLUMNREFERENCE="V"/>
		<LEDGERFAX COLUMNREFERENCE="W"/>
		<EMAIL COLUMNREFERENCE="X"/>
		<COUNTRYNAME COLUMNREFERENCE="Y"/>
		<STATENAME COLUMNREFERENCE="Z"/>
		<!--<LEDSTATENAME COLUMNREFERENCE="Z"/>-->
		<PINCODE COLUMNREFERENCE="AA"/>

		<!-- GST fields for PARTY LEDGER -->
		<GSTREGISTRATIONTYPE COLUMNREFERENCE="AF"/>
		<!-- GSTIN / UIN -->
		<PARTYGSTIN COLUMNREFERENCE="AG"/>
		<!-- Party Type -->
		<GSTNATUREOFSUPPLY COLUMNREFERENCE="AH"/>
		<ISECOMMOPERATOR COLUMNREFERENCE="AI"/>


		<!-- GST fields for SALES LEDGER -->
		<!-- Is GST Applicable -->
		<GSTAPPLICABLE SKIP="=len(trim(AJ#))=0" FORMULA="= &quot;~[~ &quot; &amp; AJ#"/>
		<!-- Type of supply -->
		<GSTTYPEOFSUPPLY COLUMNREFERENCE="AR"/>
		<!-- GST tax details -->
		<GSTDETAILS.LIST ISEMPTY="YES" SKIP="=Not(And(len(trim(AK#))=0,len(trim(AL#))=0,len(trim(AM#))=0,len(trim(AN#))=0,len(trim(AN#))=0,len(trim(AP#))=0))" />
		<GSTDETAILS.LIST SKIP="=And(len(trim(AK#))=0,len(trim(AL#))=0,len(trim(AM#))=0,len(trim(AN#))=0,len(trim(AN#))=0,len(trim(AP#))=0)">
			<!-- Applicable date -->
			<APPLICABLEFROM COLUMNREFERENCE="AK"/>
			<HSNMASTERNAME/>
			<!-- HSN/SAC code -->
			<HSNCODE COLUMNREFERENCE="AL"/>
			<!-- HSN description -->
			<HSN COLUMNREFERENCE="AM"/>
			<!-- Taxability -->
			<TAXABILITY COLUMNREFERENCE="AO"/>
			<!-- Nature of Transaction -->
			<GSTNATUREOFTRANSACTION COLUMNREFERENCE="AN"/>
			<ISREVERSECHARGEAPPLICABLE>No</ISREVERSECHARGEAPPLICABLE>
			<ISNONGSTGOODS>No</ISNONGSTGOODS>
			<GSTINELIGIBLEITC>No</GSTINELIGIBLEITC>
			<STATEWISEDETAILS.LIST>
				<STATENAME>~[~ Any</STATENAME>
				<RATEDETAILS.LIST SKIP="=len(trim(AP#))=0">
					<GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AP#/2,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST SKIP="=len(trim(AP#))=0">
					<GSTRATEDUTYHEAD>State Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AP#/2,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST SKIP="=len(trim(AP#))=0">
					<GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AP#,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST SKIP="=len(trim(AQ#))=0">
					<GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AQ#,2)"/>
				</RATEDETAILS.LIST>
				<GSTSLABRATES.LIST>        </GSTSLABRATES.LIST>
			</STATEWISEDETAILS.LIST>
			<TEMPGSTDETAILSLABRATES.LIST>       </TEMPGSTDETAILSLABRATES.LIST>
		</GSTDETAILS.LIST>

		<!-- GST fields for DUTIES AND TAXES LEDGER -->
		<!-- This tag appears for CGST only -->
		<GSTAPPLICABLE SKIP="=Or(len(trim(AT#))=0,Not(trim(AT#)=&quot;central tax&quot;))">~[~ Applicable</GSTAPPLICABLE>
		<!-- GST -->
		<TAXTYPE COLUMNREFERENCE="AS"/>
		<!-- Central Tax or State Tax or Integrated Tax -->
		<GSTDUTYHEAD COLUMNREFERENCE="AT"/>
		<!-- GST Tax Rate -->
		<RATEOFTAXCALCULATION COLUMNREFERENCE="AU"/>
		<!-- Always blank -->
		<GSTTYPE/>

		<!-- FOR VAT -->
		<TAXTYPE COLUMNREFERENCE="AV"/>
		<!-- If VAT Rate is specified and is greater than 0, then set the tag VATAPPLICABLE -->
		<VATAPPLICABLE SKIP="=Round(AX#,2)=0">~[~ Applicable</VATAPPLICABLE>
		<RATEOFTAXCALCULATION COLUMNREFERENCE="AX"/>
		<VATDETAILS.LIST SKIP="=len(trim(AW#))=0">
			<FROMDATE COLUMNREFERENCE="BA"/>
			<TAXTYPE COLUMNREFERENCE="AZ"/>
			<STATNATURENAME COLUMNREFERENCE="AW"/>
			<ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
			<ISCALCONACTUALQTY>No</ISCALCONACTUALQTY>
			<RATEOFVAT COLUMNREFERENCE="AX"/>
			<ADDITIONALTAX COLUMNREFERENCE="AY"/>
		</VATDETAILS.LIST>
		<INCOMETAXNUMBER COLUMNREFERENCE="BB"/>
		<REGISTRATIONNUMBER COLUMNREFERENCE="BC"/>
		<VATDEALERTYPE COLUMNREFERENCE="BD"/>
		<VATTINNUMBER COLUMNREFERENCE="BE"/>
		<SALESTAXNUMBER COLUMNREFERENCE="BE"/>
		<UDF:VATREGISTRATIONDATE.LIST DESC="`VATRegistrationDate`" ISLIST="YES" TYPE="Date" SKIP="=len(trim(BF#))=0">
			<UDF:VATREGISTRATIONDATE DESC="`VATRegistrationDate`" COLUMNREFERENCE="BF"/>
		</UDF:VATREGISTRATIONDATE.LIST>
		<INTERSTATESTNUMBER COLUMNREFERENCE="BG"/>
		<UDF:CSTREGISTRATIONDATE.LIST DESC="`CSTRegistrationDate`" ISLIST="YES" TYPE="Date" SKIP="=len(trim(BH#))=0">
			<UDF:CSTREGISTRATIONDATE DESC="`CSTRegistrationDate`" COLUMNREFERENCE="BH"/>
		</UDF:CSTREGISTRATIONDATE.LIST>
		<ISTDSAPPLICABLE COLUMNREFERENCE="BI"/>
		<ISSTXPARTY COLUMNREFERENCE="BJ"/>
		<PAYMENTDETAILS.LIST SKIP="=And(len(trim(BK#))=0,len(trim(BL#))=0,len(trim(BM#))=0)">
			<IFSCODE COLUMNREFERENCE="BK"/>
			<BANKNAME COLUMNREFERENCE="BM"/>
			<ACCOUNTNUMBER COLUMNREFERENCE="BL"/>
			<PAYMENTFAVOURING COLUMNREFERENCE="B"/>
			<TRANSACTIONNAME COLUMNREFERENCE="B"/>
			<SETASDEFAULT>Yes</SETASDEFAULT>
			<DEFAULTTRANSACTIONTYPE>Inter Bank Transfer</DEFAULTTRANSACTIONTYPE>
		</PAYMENTDETAILS.LIST>
		<AFFECTSSTOCK COLUMNREFERENCE="BN"/>
		<EXCISEREGNO COLUMNREFERENCE="BO"/>
		<EXCISEREGISTRATIONDATE COLUMNREFERENCE="BP"/>
		<EXCISERANGE COLUMNREFERENCE="BQ"/>
		<EXCISEDIVISION COLUMNREFERENCE="BR"/>
		<EXCISECOMMISSIONERATE COLUMNREFERENCE="BS"/>
		<ISCOSTCENTRESON COLUMNREFERENCE="BT"/>
	</MASTER>
</XMLTAGS>

