<!-- These XML tags are added to Excel sheet at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that this (i.e Column as mentioned above) is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>ID</COLUMNNAME>
		<COLUMNNAME>NAME</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!-- Settings file -->
	<DATA LOADFILE="Vouchers-V7-Purchase-with-VAT-without-stockitems-settings.csv"/>
	<!-- Create VOUCHERTYPE Master -->
	<MASTER TYPE="VOUCHERTYPE">
		<NAME.LIST TYPE="String">
			<NAME COLUMNREFERENCE="G"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="F"/>
		<MAILINGNAME FORMULA="=if(lower(trim(F#))=&quot;debit note&quot;,&quot;D.Note&quot;,&quot;Purc&quot;)"/>
		<NUMBERINGMETHOD>Manual</NUMBERINGMETHOD>
		<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(F#))=&quot;debit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
	</MASTER>
	<!-- Create Party LEDGER Masters -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="H"/>
		</NAME.LIST>
		<MAILINGNAME.LIST TYPE="String">
			<MAILINGNAME COLUMNREFERENCE="H"/>
		</MAILINGNAME.LIST>
		<PARENT>Sundry Creditors</PARENT>
		<ISBILLWISEON>Yes</ISBILLWISEON>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<COUNTRYNAME SKIP="=len(trim(I#))=0">India</COUNTRYNAME>
		<LEDSTATENAME COLUMNREFERENCE="I"/>
		<VATDEALERTYPE>Regular</VATDEALERTYPE>
	</MASTER>
	<!-- Create Purchase Ledger Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-PURCLEDGER&quot;)__&quot;"/>
		</NAME.LIST>
		<PARENT>Purchase Accounts</PARENT>
		<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-VATCLASS&quot;)__&quot;"/>
		<TAXTYPE>Others</TAXTYPE>
		<ISBILLWISEON>No</ISBILLWISEON>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<USEFORVAT>Yes</USEFORVAT>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<VATDETAILS.LIST>
			<FROMDATE>20100401</FROMDATE>
			<TAXTYPE>Taxable</TAXTYPE>
			<ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
			<ISCALCONACTUALQTY>No</ISCALCONACTUALQTY>
			<RATEOFVAT FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-VATRATE&quot;)__&quot;"/>
			<ADDITIONALTAX FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-ADDLVATRATE&quot;)__&quot;"/>
			<!--<VATCOMMODITYNAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-VATRATE&quot;)__&quot;"/>
			<VATCOMMODITYCODE FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-VATRATE&quot;)__&quot;"/>-->
		</VATDETAILS.LIST>
	</MASTER>
	<!-- Create TAX LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-VATLEDGER&quot;)__&quot;"/>
		</NAME.LIST>
		<PARENT>Duties &amp; Taxes</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-VATCLASS&quot;)__&quot;"/>
		<TAXTYPE FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-PURCTYPE&quot;)__&quot;"/>
		<BASICTYPEOFDUTY FORMULA="=if(&quot;__LOOKUP__(K# &amp; &quot;-PURCTYPE&quot;)__&quot;=&quot;CST&quot;,&quot;Sales Tax&quot;,&quot;On VAT Rate&quot;)"/>
		<SUBTAXTYPE FORMULA="=if(&quot;__LOOKUP__(K# &amp; &quot;-PURCTYPE&quot;)__&quot;=&quot;CST&quot;,&quot;&quot;,&quot;Input VAT&quot;)"/>
		<RATEOFTAXCALCULATION FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-VATRATE&quot;)__&quot;"/>
	</MASTER>
	<!-- Create ADDl. TAX LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-ADDLVATLEDGER&quot;)__&quot;"/>
		</NAME.LIST>
		<PARENT>Duties &amp; Taxes</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<TAXTYPE FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-PURCTYPE&quot;)__&quot;"/>
		<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-ADDLVATCLASS&quot;)__&quot;"/>
		<BASICTYPEOFDUTY FORMULA="=if(&quot;__LOOKUP__(K# &amp; &quot;-PURCTYPE&quot;)__&quot;=&quot;CST&quot;,&quot;Sales Tax&quot;,&quot;Additional Tax&quot;)"/>
		<SUBTAXTYPE FORMULA="=if(&quot;__LOOKUP__(K# &amp; &quot;-PURCTYPE&quot;)__&quot;=&quot;CST&quot;,&quot;&quot;,&quot;Input VAT&quot;)"/>
		<RATEOFTAXCALCULATION FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-ADDLVATRATE&quot;)__&quot;"/>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER" SINGLERECORD="YES">
		<NAME.LIST>
			<NAME>Rounding Off</NAME>
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="O"/>
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="Q"/>
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="S"/>
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="U"/>
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="W"/>
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
	</MASTER>
	<VOUCHER>
		<GUID FORMULA="=+&quot;udi-HC63AM-&quot; &amp; A# &amp; &quot;-&quot; &amp; C# &amp; &quot;-&quot; &amp; F#"/>
		<DATE COLUMNREFERENCE="D"/>
		<EFFECTIVEDATE COLUMNREFERENCE="D"/>
		<VOUCHERTYPENAME COLUMNREFERENCE="G"/>
		<VOUCHERNUMBER COLUMNREFERENCE="E"/>
		<REFERENCE COLUMNREFERENCE="B"/>
		<NARRATION COLUMNREFERENCE="J"/>
		<PARTYLEDGERNAME COLUMNREFERENCE="H"/>
		<BASICBASEPARTYNAME COLUMNREFERENCE="H"/>
		<BASICBUYERNAME COLUMNREFERENCE="H"/>
		<ISINVOICE>No</ISINVOICE>
		<PERSISTEDVIEW>Accounting Voucher View</PERSISTEDVIEW>
		<CSTFORMRECVTYPE FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-FORM&quot;)__&quot;"/>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<STATENAME COLUMNREFERENCE="I"/>
		<!-- Party Ledger -->
		<ALLLEDGERENTRIES.LIST>
			<LEDGERNAME COLUMNREFERENCE="H"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(F#[1]))=&quot;debit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
			<AMOUNT FORMULA="={Round(Y#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,-1,1)"/>
			<BILLALLOCATIONS.LIST>
				<NAME COLUMNREFERENCE="B"/>
				<BILLTYPE>New Ref</BILLTYPE>
				<AMOUNT FORMULA="={Round(Y#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,-1,1)"/>
			</BILLALLOCATIONS.LIST>
		</ALLLEDGERENTRIES.LIST>
		<!-- Purchase Ledger -->
		<ALLLEDGERENTRIES.LIST GROUPBY="=trim(K#)">
			<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-VATCLASS&quot;)__&quot;"/>
			<LEDGERNAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-PURCLEDGER&quot;)__&quot;"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(F#[1]))=&quot;debit note&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
			<AMOUNT FORMULA="={Round(L#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
		</ALLLEDGERENTRIES.LIST>
		<!-- VAT Ledger -->
		<ALLLEDGERENTRIES.LIST GROUPBY="=trim(K#)">
			<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-VATCLASS&quot;)__&quot;"/>
			<LEDGERNAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-VATLEDGER&quot;)__&quot;"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(F#[1]))=&quot;debit note&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
			<AMOUNT FORMULA="={Round(M#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
			<VATASSESSABLEVALUE FORMULA="={Round(L#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
			<UDF:ADDNLVATCLASSAMT.LIST DESC="`AddnlVATClassAmt`" ISLIST="YES" TYPE="Amount" INDEX="10004">
				<UDF:ADDNLVATCLASSAMT DESC="`AddnlVATClassAmt`" FORMULA="={Round(L#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
			</UDF:ADDNLVATCLASSAMT.LIST>
		</ALLLEDGERENTRIES.LIST>
		<!-- Addl VAT Ledger -->
		<ALLLEDGERENTRIES.LIST GROUPBY="=trim(K#)" SKIP="={Round(N#,2)}=0">
			<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-ADDLVATCLASS&quot;)__&quot;"/>
			<LEDGERNAME FORMULA="=&quot;__LOOKUP__(K# &amp; &quot;-ADDLVATLEDGER&quot;)__&quot;"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(F#[1]))=&quot;debit note&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
			<AMOUNT FORMULA="={Round(N#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
			<VATASSESSABLEVALUE FORMULA="={Round(L#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
			<UDF:ADDNLVATCLASSAMT.LIST DESC="`AddnlVATClassAmt`" ISLIST="YES" TYPE="Amount" INDEX="10004">
				<UDF:ADDNLVATCLASSAMT DESC="`AddnlVATClassAmt`" FORMULA="={Round(L#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
			</UDF:ADDNLVATCLASSAMT.LIST>
		</ALLLEDGERENTRIES.LIST>
		<!-- Ledger1 -->
		<LEDGERENTRIES.LIST SKIP="={Round(P#,2)}=0">
			<LEDGERNAME COLUMNREFERENCE="O"/>
			<!---
			Purchase
			a) P < 0,		No,			Credit
			b) P > 0,		Yes,		Debit
			Debit Note
			a) P < 0,		Yes,		Debit
			b) P > 0,		No,			Credit

			if(lower(trim(F#[1]))=&quot;debit note&quot;,
			If({Round(P#,2)}&lt;0,&quot;Yes&quot;,&quot;No&quot;),
			If({Round(P#,2)}&lt;0,&quot;No&quot;,&quot;Yes&quot;)
			)
			-->
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(F#[1]))=&quot;debit note&quot;,If({Round(P#,2)}&lt;0,&quot;Yes&quot;,&quot;No&quot;),If({Round(P#,2)}&lt;0,&quot;No&quot;,&quot;Yes&quot;))"/>
			<AMOUNT FORMULA="={Round(P#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
		</LEDGERENTRIES.LIST>		
		<!-- Ledger2 -->
		<LEDGERENTRIES.LIST SKIP="={Round(R#,2)}=0">
			<LEDGERNAME COLUMNREFERENCE="Q"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(F#[1]))=&quot;debit note&quot;,If({Round(P#,2)}&lt;0,&quot;Yes&quot;,&quot;No&quot;),If({Round(P#,2)}&lt;0,&quot;No&quot;,&quot;Yes&quot;))"/>
			<AMOUNT FORMULA="={Round(R#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
		</LEDGERENTRIES.LIST>		
		<!-- Ledger3 -->
		<LEDGERENTRIES.LIST SKIP="={Round(T#,2)}=0">
			<LEDGERNAME COLUMNREFERENCE="S"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(F#[1]))=&quot;debit note&quot;,If({Round(P#,2)}&lt;0,&quot;Yes&quot;,&quot;No&quot;),If({Round(P#,2)}&lt;0,&quot;No&quot;,&quot;Yes&quot;))"/>
			<AMOUNT FORMULA="={Round(T#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
		</LEDGERENTRIES.LIST>		
		<!-- Ledger4 -->
		<LEDGERENTRIES.LIST SKIP="={Round(V#,2)}=0">
			<LEDGERNAME COLUMNREFERENCE="U"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(F#[1]))=&quot;debit note&quot;,If({Round(P#,2)}&lt;0,&quot;Yes&quot;,&quot;No&quot;),If({Round(P#,2)}&lt;0,&quot;No&quot;,&quot;Yes&quot;))"/>
			<AMOUNT FORMULA="={Round(V#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
		</LEDGERENTRIES.LIST>		
		<!-- Ledger5 -->
		<LEDGERENTRIES.LIST SKIP="={Round(X#,2)}=0">
			<LEDGERNAME COLUMNREFERENCE="W"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(F#[1]))=&quot;debit note&quot;,If({Round(P#,2)}&lt;0,&quot;Yes&quot;,&quot;No&quot;),If({Round(P#,2)}&lt;0,&quot;No&quot;,&quot;Yes&quot;))"/>
			<AMOUNT FORMULA="={Round(X#,2)}*if(lower(trim(F#[1]))=&quot;debit note&quot;,1,-1)"/>
		</LEDGERENTRIES.LIST>		
		
		<UDF:REFERENCEDATE.LIST DESC="`ReferenceDate`" ISLIST="YES" TYPE="Date">
			<UDF:REFERENCEDATE DESC="`ReferenceDate`" COLUMNREFERENCE="C"/>
		</UDF:REFERENCEDATE.LIST>
	</VOUCHER>
</XMLTAGS>

