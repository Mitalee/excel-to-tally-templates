<!-- These XML tags are added to Excel sheet at run-time by UDIMAGIC  -->
<!-- 
New Features in v5.0
1) FROMROW attribute
   In the earlier version (prior to v5.0), it was presumed that HEADINGS are in ROW 1 and DATA starts from ROW 2. Now, you can use the FROMROW attribute to 
   specify the Excel sheet ROW Number from where the DATA begins.
2) SCROLLHORIZONTALLY attribute
   The SCROLLHORIZONTALLY attribute uses an internal mechanism which increments the COLUMN in the specified FORMULA. This enables you to have virtually 
   any number of COLUMNs for your DATA.
3) FORMULA2 attribute
   
-->
<XMLTAGS CELLREFERENCE="A1" FROMROW="12" xmlns:UDF="TallyUDF">
	<!-- Specifies that this (i.e Column as mentioned above) is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>ID</COLUMNNAME>
		<COLUMNNAME>NAME</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!-- Create Vouchertype Masters -->
	<MASTER TYPE="VOUCHERTYPE">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="D"/>
		</NAME.LIST>
		<PARENT FORMULA="=if(Or(lower(C#)=&quot;sales&quot;,len(trim(C#))=0),&quot;Sales&quot;,C#)"/>
		<ADDITIONALNAME FORMULA="=if(Or(lower(C#)=&quot;sales&quot;,len(trim(C#))=0),&quot;Sale&quot;,&quot;Cr.Note&quot;)"/>
		<NUMBERINGMETHOD>Manual</NUMBERINGMETHOD>
		<ISDEEMEDPOSITIVE FORMULA="=if(Or(lower(C#)=&quot;sales&quot;,len(trim(C#))=0),&quot;Yes&quot;,&quot;No&quot;)"/>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<PREVENTDUPLICATES>Yes</PREVENTDUPLICATES>
		<PREFILLZERO>No</PREFILLZERO>
		<PRINTAFTERSAVE>No</PRINTAFTERSAVE>
		<FORMALRECEIPT>No</FORMALRECEIPT>
		<COMMONNARRATION>Yes</COMMONNARRATION>
	</MASTER>
	<!-- 
                The SCROLLHORIZONTALLY attribute specifies the condition (any valid Excel formula). Herein, the condition is to check the length of cell L8
                and if it is greater than 0, a Ledger Master is created (based on value in L8). The loop executes and now L8 becomes M8.
                If length of cell M8 is greater than 0, Ledger Master is created (based on value given in M8). The loop go on (say N8, O8, P8... till the
                conditions becomes false.
    -->
	<!-- Create Ledger Masters -->
	<MASTER TYPE="LEDGER" SINGLERECORD="YES" SCROLLHORIZONTALLY="=Len(trim(V$11))&gt;0">
		<NAME.LIST>
			<NAME FORMULA="=V$11"/>
		</NAME.LIST>
		<PARENT FORMULA="=V$10"/>
		<TAXCLASSIFICATIONNAME SKIP="=Len(trim(V$6))=0" FORMULA="=V$6"/>
		<!-- Yes for Sales ledgers. No for others -->
		<USEFORVAT SKIP="=Len(trim(V$1))=0" FORMULA="=V$1"/>
		<AFFECTSSTOCK SKIP="=Len(trim(V$1))=0" FORMULA="=V$1"/>
		<!-- Required for VAT and CST both -->
		<TAXTYPE SKIP="=Len(trim(V$4))=0" FORMULA="=V$4"/>
		<!-- For VAT only -->
		<BASICTYPEOFDUTY SKIP="=Or(Len(trim(V$4))=0,Not(lower(V$4)=&quot;vat&quot;))">On VAT Rate</BASICTYPEOFDUTY>
		<!-- For VAT only -->
		<SUBTAXTYPE SKIP="=Or(Len(trim(V$4))=0,Not(lower(V$4)=&quot;vat&quot;))">Output VAT</SUBTAXTYPE>
		<!-- CostCentre Required or not -->
		<ISCOSTCENTRESON SKIP="=Len(trim(V$1))=0" FORMULA="=V$9"/>
		<!-- For VAT only -->
		<RATEOFTAXCALCULATION SKIP="=Or(Len(trim(V$4))=0,Not(lower(V$4)=&quot;vat&quot;))" FORMULA="=V$2"/>
		<!-- For Sales Ledger only. Required for Tally ERP 9 Release 5.1 and higher -->
		<VATDETAILS.LIST SKIP="=not(lower(V$1)=&quot;yes&quot;)">
			<FROMDATE>20100401</FROMDATE>
			<TAXTYPE>Taxable</TAXTYPE>
			<ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
			<ISCALCONACTUALQTY>No</ISCALCONACTUALQTY>
			<RATEOFVAT FORMULA="=V$2"/>
			<!--<ADDITIONALTAX></ADDITIONALTAX>
			<VATCOMMODITYNAME></VATCOMMODITYNAME>
			<VATCOMMODITYCODE></VATCOMMODITYCODE>-->
		</VATDETAILS.LIST>
	</MASTER>
	<!-- Create Party Ledger Master-->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="E"/>
		</NAME.LIST>
		<PARENT>Sundry Debtors</PARENT>
		<ISBILLWISEON>Yes</ISBILLWISEON>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<USEFORVAT>No</USEFORVAT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<COUNTRYNAME SKIP="=len(trim(F#))=0">India</COUNTRYNAME>
		<LEDSTATENAME COLUMNREFERENCE="F"/>
		<VATDEALERTYPE>Unregistered</VATDEALERTYPE>
	</MASTER>
	<!-- Create COSTCATEGORY Master-->
	<MASTER TYPE="COSTCATEGORY">
		<NAME.LIST TYPE="String">
			<NAME COLUMNREFERENCE="Q"/>
		</NAME.LIST>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<ALLOCATEREVENUE>Yes</ALLOCATEREVENUE>
		<ALLOCATENONREVENUE>No</ALLOCATENONREVENUE>
	</MASTER>
	<!-- Create COSTCENTRE Master-->
	<MASTER TYPE="COSTCENTRE">
		<NAME.LIST TYPE="String">
			<NAME COLUMNREFERENCE="R"/>
		</NAME.LIST>
		<PARENT/>
		<CATEGORY COLUMNREFERENCE="Q"/>
		<REVENUELEDFOROPBAL>No</REVENUELEDFOROPBAL>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<FORPAYROLL>No</FORPAYROLL>
		<FORJOBCOSTING>No</FORJOBCOSTING>
		<ISEMPLOYEEGROUP>No</ISEMPLOYEEGROUP>
	</MASTER>
	<!-- Create UNIT Masters -->
	<MASTER TYPE="UNIT">
		<NAME COLUMNREFERENCE="H"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create STOCKITEM Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST TYPE="String">
			<NAME COLUMNREFERENCE="G"/>
		</NAME.LIST>
		<BASEUNITS COLUMNREFERENCE="H"/>
		<ISBATCHWISEON FORMULA="=if(Len(trim(P#))=0,&quot;No&quot;,&quot;Yes&quot;)"/>
	</MASTER>
	<!-- Create GODOWN Masters -->
	<MASTER TYPE="GODOWN">
		<NAME.LIST TYPE="String">
			<NAME COLUMNREFERENCE="O"/>
		</NAME.LIST>
	</MASTER>
	<!-- SALES VOUCHER-->
	<VOUCHER>
		<GUID FORMULA="=+&quot;udi-HD59B2-&quot; &amp; A# &amp; &quot;-&quot; &amp; B# &amp; &quot;-&quot; &amp; D#"/>
		<!--GUID FORMULA="=+&quot;udi-sale-&quot; &amp; A# &amp; &quot;-&quot; &amp; B#" /-->
		<DATE COLUMNREFERENCE="B"/>
		<EFFECTIVEDATE COLUMNREFERENCE="B"/>
		<VOUCHERTYPENAME COLUMNREFERENCE="D"/>
		<ISINVOICE>Yes</ISINVOICE>
		<PERSISTEDVIEW>Invoice Voucher View</PERSISTEDVIEW>
		<PARTYLEDGERNAME COLUMNREFERENCE="E"/>
		<PARTYNAME COLUMNREFERENCE="E"/>
		<BASICBASEPARTYNAME COLUMNREFERENCE="E"/>
		<BASICBUYERNAME COLUMNREFERENCE="E"/>
		<REFERENCE COLUMNREFERENCE="A"/>
		<VOUCHERNUMBER COLUMNREFERENCE="S"/>
		<CSTFORMISSUETYPE COLUMNREFERENCE="T"/>
		<CSTFORMRECVTYPE COLUMNREFERENCE="U"/>
		<NARRATION COLUMNREFERENCE="N"/>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<STATENAME COLUMNREFERENCE="F"/>
		
		<!-- Party Ledger to be Debited / Credited-->
		<LEDGERENTRIES.LIST>
			<LEDGERNAME COLUMNREFERENCE="E"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(C#[1]))=&quot;credit note&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
			<!-- Running Totals are computed using {} -->
			<AMOUNT FORMULA="={Round(M#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,1,-1)"/>
			<BILLALLOCATIONS.LIST>
				<NAME COLUMNREFERENCE="A"/>
				<BILLTYPE>New Ref</BILLTYPE>
				<!-- Running Totals are computed using {} -->
				<AMOUNT FORMULA="={Round(M#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,1,-1)"/>
			</BILLALLOCATIONS.LIST>
		</LEDGERENTRIES.LIST>
		<!-- VAT and other Ledgers -->
		<LEDGERENTRIES.LIST SKIP="=Or(V$1=&quot;Yes&quot;,{Round(V#,2)}=0)" SCROLLHORIZONTALLY="=Len(trim(V$11))&gt;0">
			<BASICRATEOFINVOICETAX.LIST SKIP="=Round(V$2,2)=0" TYPE="Number">
				<BASICRATEOFINVOICETAX FORMULA="=V$2"/>
			</BASICRATEOFINVOICETAX.LIST>
			<TAXCLASSIFICATIONNAME SKIP="=Len(trim(V$6))=0" FORMULA="=V$6"/>
			<LEDGERNAME FORMULA="=V$11"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim($C#[1]))=&quot;credit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
			<AMOUNT FORMULA="={Round(V#,2)}*if(lower(trim($C#[1]))=&quot;credit note&quot;,-1,1)"/>
			<VATASSESSABLEVALUE SKIP="=Len(trim(V$5))=0" FORMULA2="=V$5"/>
			<!-- Required when VAT Class is specified -->
			<TAXOBJECTALLOCATIONS.LIST SKIP="=Or(Len(trim(V$6))=0,Len(trim(V$4))=0)">
				<CATEGORY SKIP="=Len(V$6)=0" FORMULA="=V$6"/>
				<TAXTYPE FORMULA="=V$4"/>
				<!-- Do not increment Column Reference. Bill-Name is always in said Column -->
				<TAXNAME FORMULA="=$A#"/>
				<!-- Do not increment Column Reference. Party-Name is always in said Column -->
				<PARTYLEDGER FORMULA="=$E#"/>
				<REFTYPE>New Ref</REFTYPE>
				<ISOPTIONAL>No</ISOPTIONAL>
				<ISPANVALID>No</ISPANVALID>
				<ZERORATED>No</ZERORATED>
				<EXEMPTED>No</EXEMPTED>
				<ISSPECIALRATE>No</ISSPECIALRATE>
				<ISDEDUCTNOW>No</ISDEDUCTNOW>
				<ISPANNOTAVAILABLE>No</ISPANNOTAVAILABLE>
				<SUBCATEGORYALLOCATION.LIST SCROLL="Yes" SKIP="=Or(Len($G#)=0,Round(V#,2)=0)">
					<!-- Do not increment Column Reference. Item-Name is always in said Column -->
					<STOCKITEMNAME FORMULA="=$G#"/>
					<SUBCATEGORY FORMULA="=V$4"/>
					<DUTYLEDGER FORMULA="=V$11"/>
					<SUBCATZERORATED>No</SUBCATZERORATED>
					<SUBCATEXEMPTED>No</SUBCATEXEMPTED>
					<SUBCATISSPECIALRATE>No</SUBCATISSPECIALRATE>
					<ASSESSABLEAMOUNT FORMULA2="=V$5"/>
					<TAX FORMULA="=Round(V#,2)*if(lower(trim($C#[1]))=&quot;credit note&quot;,-1,1)"/>
					<!-- Do not increment Column Reference. Quantity is to be always taken for said Column -->
					<BILLEDQTY FORMULA="=$I#"/>
				</SUBCATEGORYALLOCATION.LIST>
			</TAXOBJECTALLOCATIONS.LIST>
			<CATEGORYALLOCATIONS.LIST SKIP="=Or(Len(trim($R#))=0,Not(upper(V$9)=&quot;YES&quot;))">
				<CATEGORY FORMULA="=If(len(trim($Q#))=0,&quot;Primary Cost Category&quot;,$Q#)"/>
				<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim($C#[1]))=&quot;credit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
				<COSTCENTREALLOCATIONS.LIST>
					<NAME FORMULA="=$R#"/>
					<AMOUNT FORMULA="={Round(V#,2)}*if(lower(trim($C#[1]))=&quot;credit note&quot;,-1,1)"/>
				</COSTCENTREALLOCATIONS.LIST>
			</CATEGORYALLOCATIONS.LIST>
			<UDF:ADDNLVATCLASSAMT.LIST DESC="`AddnlVATClassAmt`" ISLIST="YES" TYPE="Amount" SKIP="=Not(V$4=&quot;VAT&quot;)">
				<UDF:ADDNLVATCLASSAMT DESC="`AddnlVATClassAmt`" FORMULA2="=V$5"/>
			</UDF:ADDNLVATCLASSAMT.LIST>
			<UDF:MAHWCASSESSABLEVALUE.LIST DESC="`MAHWCAssessableValue`" ISLIST="YES" TYPE="Amount" SKIP="=Not(V$4=&quot;VAT&quot;)">
				<UDF:MAHWCASSESSABLEVALUE DESC="`MAHWCAssessableValue`" FORMULA2="=V$5"/>
			</UDF:MAHWCASSESSABLEVALUE.LIST>
		</LEDGERENTRIES.LIST>
		<!-- STOCKITEMS -->
		<!-- SCROLL="Yes" means scroll vertically i.e. row-wise -->
		<ALLINVENTORYENTRIES.LIST SCROLL="Yes" SKIP="=Len(trim(G#))=0">
			<STOCKITEMNAME COLUMNREFERENCE="G"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim($C#[1]))=&quot;credit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
			<RATE FORMULA="=Round(J#,2)"/>
			<AMOUNT FORMULA="=Round(K#,2)*if(lower(trim($C#[1]))=&quot;credit note&quot;,-1,1)"/>
			<ACTUALQTY COLUMNREFERENCE="I"/>
			<BILLEDQTY COLUMNREFERENCE="I"/>
			<BATCHALLOCATIONS.LIST>
				<GODOWNNAME FORMULA="=If(len(trim(O#))=0,&quot;Main Location&quot;,O#)"/>
				<BATCHNAME FORMULA="=if(Len(trim(P#))=0,&quot;Primary Batch&quot;,P#)"/>
				<!--BATCHNAME>Primary Batch</BATCHNAME-->
				<AMOUNT FORMULA="=Round(K#,2)*if(lower(trim($C#[1]))=&quot;credit note&quot;,-1,1)"/>
				<ACTUALQTY COLUMNREFERENCE="I"/>
				<BILLEDQTY COLUMNREFERENCE="I"/>
			</BATCHALLOCATIONS.LIST>
			<!-- SCROLLHORIZONTALLY attribute is used herein -->
			<ACCOUNTINGALLOCATIONS.LIST SCROLLHORIZONTALLY="=Len(trim(V$11))&gt;0" SKIP="=Or(Not($L#=V$3),Not(V$1=&quot;Yes&quot;))">
				<!-- Row number is fixed. Column will be incremented at run-time -->
				<TAXCLASSIFICATIONNAME SKIP="=Len(trim(V$6))=0" FORMULA="=V$6"/>
				<!-- Row number is fixed. Column will be incremented at run-time -->
				<LEDGERNAME FORMULA="=V$11"/>
				<!--ISDEEMEDPOSITIVE>No</ISDEEMEDPOSITIVE-->
				<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim($C#[1]))=&quot;credit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
				<!-- Column is Fixed. Row number will be incremented at run-time -->
				<AMOUNT FORMULA="=Round($K#,2)*if(lower(trim($C#[1]))=&quot;credit note&quot;,-1,1)"/>
				<CATEGORYALLOCATIONS.LIST SKIP="=Or(Len(trim($R#))=0,Not(upper(V$9)=&quot;YES&quot;))">
					<CATEGORY FORMULA="=If(len(trim($Q#))=0,&quot;Primary Cost Category&quot;,$Q#)"/>
					<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim($C#[1]))=&quot;credit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
					<COSTCENTREALLOCATIONS.LIST>
						<NAME FORMULA="=$R#"/>
						<AMOUNT FORMULA="=Round($K#,2)*if(lower(trim($C#[1]))=&quot;credit note&quot;,-1,1)"/>
					</COSTCENTREALLOCATIONS.LIST>
				</CATEGORYALLOCATIONS.LIST>
			</ACCOUNTINGALLOCATIONS.LIST>
		</ALLINVENTORYENTRIES.LIST>
	</VOUCHER>
</XMLTAGS>

