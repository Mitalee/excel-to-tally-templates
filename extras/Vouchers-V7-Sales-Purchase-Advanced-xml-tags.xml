			<VATASSESSABLEVALUE FORMULA="={Round(Round(AQ#,2)+Round(AS#,2)+Round(AU#,2),2)}*if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),-1,1)"/>
			<UDF:ADDNLVATCLASSAMT.LIST DESC="`AddnlVATClassAmt`" ISLIST="YES" TYPE="Amount" ISGROUPED="YES">
				<UDF:ADDNLVATCLASSAMT DESC="`AddnlVATClassAmt`" FORMULA="={Round(Round(AQ#,2)+Round(AS#,2)+Round(AU#,2),2)}*if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),-1,1)"/>
			</UDF:ADDNLVATCLASSAMT.LIST>
			<TAXOBJECTALLOCATIONS.LIST>
