<!-- These XML tags are added to Excel sheet at run-time by UDIMAGIC  -->
<!-- 
	\123 is replaced with { at runtime 
	\125 is replaced with } at runtime
	The curly braces are used for Array notation in Excel
-->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that this (i.e Column as mentioned above) is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>ID</COLUMNNAME>
		<COLUMNNAME>NAME</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!-- Create VOUCHERTYPE Master -->
	<MASTER TYPE="VOUCHERTYPE">
		<NAME.LIST TYPE="String">
			<NAME COLUMNREFERENCE="C"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="B"/>
		<ISDEEMEDPOSITIVE FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),&quot;No&quot;,&quot;Yes&quot;)"/>
		<NUMBERINGMETHOD>Manual</NUMBERINGMETHOD>
	</MASTER>
	<!-- Create Party LEDGER Masters -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="H"/>
		</NAME.LIST>
		<ADDITIONALNAME COLUMNREFERENCE="H"/>
		<PARENT FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;Sundry Creditors&quot;,&quot;Sundry Debtors&quot;)"/>
		<ISBILLWISEON>Yes</ISBILLWISEON>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<COUNTRYNAME SKIP="=len(trim(Y#))=0">India</COUNTRYNAME>
		<LEDSTATENAME COLUMNREFERENCE="Y"/>
		<VATDEALERTYPE SKIP="=len(trim(Y#))=0" COLUMNREFERENCE="I"/>
		<VATTINNUMBER SKIP="=len(trim(Y#))=0" COLUMNREFERENCE="J"/>
	</MASTER>
	<!-- Create Sales/Purchase Ledger Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="AY"/>
		</NAME.LIST>
		<PARENT FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;Purchase Accounts&quot;,&quot;Sales Accounts&quot;)"/>
		<TAXCLASSIFICATIONNAME COLUMNREFERENCE="BE"/>
		<TAXTYPE>Others</TAXTYPE>
		<ISBILLWISEON>No</ISBILLWISEON>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK FORMULA="=if(len(trim(AK#))=0,&quot;No&quot;,&quot;Yes&quot;)"/>
		<USEFORVAT>Yes</USEFORVAT>
		<VATAPPLICABLE>Applicable</VATAPPLICABLE>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<VATDETAILS.LIST>
			<FROMDATE>20100401</FROMDATE>
			<TAXTYPE>Taxable</TAXTYPE>
			<STATNATURENAME COLUMNREFERENCE="BB"/>
			<ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
			<ISCALCONACTUALQTY>No</ISCALCONACTUALQTY>
			<RATEOFVAT COLUMNREFERENCE="AW"/>
			<ADDITIONALTAX COLUMNREFERENCE="AX"/>
			<VATCOMMODITYNAME COLUMNREFERENCE="AW"/>
			<VATCOMMODITYCODE COLUMNREFERENCE="AW"/>
		</VATDETAILS.LIST>
	</MASTER>
	<!-- Create TAX LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<!--<NAME FORMULA="=&quot;__LOOKUP__(AV# &amp; &quot;-VATLEDGER-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>-->
			<NAME COLUMNREFERENCE="AZ"/>
		</NAME.LIST>
		<PARENT>Duties &amp; Taxes</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<TAXCLASSIFICATIONNAME COLUMNREFERENCE="BE"/>
		<TAXTYPE COLUMNREFERENCE="AV"/>
		<BASICTYPEOFDUTY FORMULA="=if(trim(AV#)=&quot;CST&quot;,&quot;Sales Tax&quot;,&quot;On VAT Rate&quot;)"/>
		<SUBTAXTYPE FORMULA="=if(trim(AV#)=&quot;CST&quot;,&quot;&quot;,if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;Input VAT&quot;,&quot;Output VAT&quot;))"/>
		<RATEOFTAXCALCULATION COLUMNREFERENCE="AW"/>
	</MASTER>
	<!-- Create ADDl. TAX LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<!--<NAME FORMULA="=&quot;__LOOKUP__(AV# &amp; &quot;-ADDLVATLEDGER-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>-->
			<NAME COLUMNREFERENCE="BA"/>
		</NAME.LIST>
		<PARENT>Duties &amp; Taxes</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<TAXTYPE COLUMNREFERENCE="AV"/>
		<TAXCLASSIFICATIONNAME COLUMNREFERENCE="BF"/>
		<BASICTYPEOFDUTY FORMULA="=if(trim(AV#)=&quot;CST&quot;,&quot;Sales Tax&quot;,&quot;Additional Tax&quot;)"/>
		<SUBTAXTYPE FORMULA="=if(trim(AV#)=&quot;CST&quot;,&quot;&quot;,if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;Input VAT&quot;,&quot;Output VAT&quot;))"/>
		<RATEOFTAXCALCULATION COLUMNREFERENCE="AX"/>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER" SINGLERECORD="YES">
		<NAME.LIST>
			<NAME>Rounding Off</NAME>
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<!-- Ledger before VAT -->
			<NAME COLUMNREFERENCE="AR"/>
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
		<!-- For Tally ERP 9 Release 5.1 or higher -->
		<TAXTYPE>Others</TAXTYPE>
		<APPROPRIATEFOR>VAT</APPROPRIATEFOR>
		<EXCISEALLOCTYPE>Based on Value</EXCISEALLOCTYPE>
		<ISEXCISEAPPLICABLE>Yes</ISEXCISEAPPLICABLE>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<!-- Ledger before VAT -->
			<NAME COLUMNREFERENCE="AT"/>
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
		<!-- For Tally ERP 9 Release 5.1 or higher -->
		<TAXTYPE>Others</TAXTYPE>
		<APPROPRIATEFOR>VAT</APPROPRIATEFOR>
		<EXCISEALLOCTYPE>Based on Value</EXCISEALLOCTYPE>
		<ISEXCISEAPPLICABLE>Yes</ISEXCISEAPPLICABLE>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="BI"/>
			<!--<NAME>Shipping Charges</NAME>-->
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="BK"/>
			<!--<NAME>Other Charges1</NAME>-->
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
	</MASTER>
	<!-- Create LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="BM"/>
			<!--<NAME>Other Charges2</NAME>-->
		</NAME.LIST>
		<PARENT>Indirect Expenses</PARENT>
	</MASTER>
	<!-- Create UNIT Masters -->
	<MASTER TYPE="UNIT">
		<NAME COLUMNREFERENCE="AM"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create Godown Masters -->
	<MASTER TYPE="GODOWN">
		<NAME COLUMNREFERENCE="AG"/>
		<PARENT/>
	</MASTER>
	<!-- Create STOCKITEM Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="AK"/>
		</NAME.LIST>
		<PARENT/>
		<BASEUNITS COLUMNREFERENCE="AM"/>
		<ISBATCHWISEON FORMULA="=if((len(trim(AH#))+len(trim(AI#))+len(trim(AJ#)))=0,&quot;No&quot;,&quot;Yes&quot;)"/>
		<HASMFGDATE FORMULA="=if(len(trim(AI#))=0,&quot;No&quot;,&quot;Yes&quot;)"/>
		<ISPERISHABLEON FORMULA="=if(len(trim(AJ#))=0,&quot;No&quot;,&quot;Yes&quot;)"/>
	</MASTER>
	<VOUCHER>
		<GUID FORMULA="=+&quot;udi-HD7Q5X-&quot; &amp; A# &amp; &quot;-&quot; &amp; E# &amp; &quot;-&quot; &amp; C#"/>
		<DATE COLUMNREFERENCE="E"/>
		<EFFECTIVEDATE COLUMNREFERENCE="E"/>
		<VOUCHERTYPENAME COLUMNREFERENCE="C"/>
		<VOUCHERNUMBER COLUMNREFERENCE="D"/>
		<REFERENCE COLUMNREFERENCE="F"/>
		<UDF:REFERENCEDATE.LIST DESC="`ReferenceDate`" ISLIST="YES" TYPE="Date" INDEX="10000">
			<UDF:REFERENCEDATE DESC="`ReferenceDate`" COLUMNREFERENCE="G"/>
		</UDF:REFERENCEDATE.LIST>
		<REFERENCEDATE COLUMNREFERENCE="G"/>
		<NARRATION COLUMNREFERENCE="BO"/>
		<PARTYNAME COLUMNREFERENCE="H"/>
		<PARTYLEDGERNAME COLUMNREFERENCE="H"/>
		<BASICBASEPARTYNAME COLUMNREFERENCE="H"/>
		<BASICBUYERNAME COLUMNREFERENCE="Z"/>
		<!-- Despatch through -->
		<BASICSHIPPEDBY COLUMNREFERENCE="L"/>
		<!-- Despatch Document No -->
		<BASICSHIPDOCUMENTNO COLUMNREFERENCE="K"/>
		<!-- Destination -->
		<BASICFINALDESTINATION COLUMNREFERENCE="M"/>
		<!-- No column in Excel for this. Hence, taken from REF -->
		<BASICORDERREF COLUMNREFERENCE="F"/>
		<BASICDUEDATEOFPYMT COLUMNREFERENCE="R"/>
		<PERSISTEDVIEW FORMULA="=if(len(trim(AK#))=0,&quot;Accounting Voucher View&quot;,&quot;Invoice Voucher View&quot;)"/>
		<ISINVOICE>Yes</ISINVOICE>
		<ADDRESS.LIST TYPE="String" SKIP="=len(trim(T#))=0">
			<ADDRESS COLUMNREFERENCE="T"/>
			<ADDRESS COLUMNREFERENCE="U"/>
			<ADDRESS COLUMNREFERENCE="V"/>
			<ADDRESS COLUMNREFERENCE="W"/>
			<ADDRESS COLUMNREFERENCE="X"/>
			<ADDRESS COLUMNREFERENCE="Y"/>
		</ADDRESS.LIST>
		<BASICBUYERADDRESS.LIST TYPE="String" SKIP="=len(trim(AA#))=0">
			<BASICBUYERADDRESS COLUMNREFERENCE="AA"/>
			<BASICBUYERADDRESS COLUMNREFERENCE="AB"/>
			<BASICBUYERADDRESS COLUMNREFERENCE="AC"/>
			<BASICBUYERADDRESS COLUMNREFERENCE="AD"/>
			<BASICBUYERADDRESS COLUMNREFERENCE="AE"/>
			<BASICBUYERADDRESS COLUMNREFERENCE="AF"/>
		</BASICBUYERADDRESS.LIST>
		<BASICORDERTERMS.LIST TYPE="String" SKIP="=len(trim(R#))=0">
			<BASICORDERTERMS COLUMNREFERENCE="R"/>
		</BASICORDERTERMS.LIST>
		<!-- For Sales and Purchase only only -->
		<INVOICEDELNOTES.LIST SCROLL="YES" SKIP="=not(And(Or(len(trim(P#))&gt;0,len(trim(Q#))&gt;0),Or(trim(lower(B#[1]))=&quot;sales&quot;,trim(lower(B#[1]))=&quot;purchase&quot;)))">
			<BASICSHIPPINGDATE COLUMNREFERENCE="Q"/>
			<BASICSHIPDELIVERYNOTE COLUMNREFERENCE="P"/>
		</INVOICEDELNOTES.LIST>
		<!-- Order list -->
		<!-- For Delivery Note, Sales Invoice
				 Receipt Note, Purchase Invoice
		-->
		<INVOICEORDERLIST.LIST SCROLL="YES" SKIP="=Or(len(trim(N#))=0,trim(lower(B#[1]))=&quot;sales order&quot;,trim(lower(B#[1]))=&quot;purchase order&quot;)">
			<BASICORDERDATE COLUMNREFERENCE="O"/>
			<BASICPURCHASEORDERNO COLUMNREFERENCE="N"/>
		</INVOICEORDERLIST.LIST>
		<STATADJUSTMENTTYPE COLUMNREFERENCE="BT"/>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<STATENAME COLUMNREFERENCE="Y"/>
		<VATDEALERTYPE COLUMNREFERENCE="I"/>
		<!-- For Purchase -->
		<UDF:VATDEALERNATURE.LIST DESC="`VATDealerNature`" ISLIST="YES" TYPE="String" SKIP="=len(trim(I#))=0">
			<UDF:VATDEALERNATURE DESC="`VATDealerNature`" COLUMNREFERENCE="I"/>
		</UDF:VATDEALERNATURE.LIST>
		<BASICBUYERSSALESTAXNO COLUMNREFERENCE="J"/>
		
		<!-- For Tally ERP 9 Release 5.1 or higher -->
		<VATTYPEOFDEVICE COLUMNREFERENCE="BC"/>
		<CSTFORMRECVTYPE SKIP="=Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0)))" COLUMNREFERENCE="BD"/>
		<CSTFORMISSUETYPE SKIP="=(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0)))" COLUMNREFERENCE="BD"/>
		
		<!-- Sales : Party Ledger to be debited -->
		<!-- Purchase : Party Ledger to be Credited -->
		<LEDGERENTRIES.LIST ALTERNATETAGNAME="=if(len(trim(AK#))=0,&quot;ALLLEDGERENTRIES.LIST&quot;,&quot;&quot;)">
			<LEDGERNAME COLUMNREFERENCE="H"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),&quot;No&quot;,&quot;Yes&quot;)"/>
			<AMOUNT FORMULA="={Round(Round(AQ#,2)+Round(BG#,2)+Round(BH#,2)+Round(AS#+AU#+BJ#+BL#+BN#,2),2)}*if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),1,-1)"/>
			<BILLALLOCATIONS.LIST GROUPBY="=trim(BQ#)" SKIP="=Or(Len(trim(BQ#))=0,Round(Round(AQ#,2)+Round(BG#,2)+Round(BH#,2)+Round(AS#+AU#+BJ#+BL#+BN#,2),2)=0)">
				<!--BILLALLOCATIONS.LIST SCROLL="YES" SKIP="=Or(Len(trim(BQ#))=0,Round(Round(AQ#,2)+Round(BG#,2)+Round(BH#,2)+Round(AS#+AU#+BJ#+BL#+BN#,2),2)=0)"-->
				<NAME COLUMNREFERENCE="BQ"/>
				<BILLTYPE COLUMNREFERENCE="BP"/>
				<BILLCREDITPERIOD COLUMNREFERENCE="BS"/>
				<AMOUNT FORMULA="={Round(BR#,2)}*if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),1,-1)"/>
				<!--<AMOUNT FORMULA="=Round(BR#,2)*if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),1,-1)"/>-->
				<!--<AMOUNT FORMULA="={Round(Round(AQ#,2)+Round(BG#,2)+Round(BH#,2)+Round(AS#+AU#+BJ#+BL#+BN#,2),2)}*if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),1,-1)"/>-->
				<!--AMOUNT FORMULA="=Round(Round(AQ#,2)+Round(BG#,2)+Round(BH#,2)+Round(AS#+AU#+BJ#+BL#+BN#,2),2)*if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),1,-1)"/-->
			</BILLALLOCATIONS.LIST>
		</LEDGERENTRIES.LIST>
		<!-- Ledger1 Before VAT -->
		<LEDGERENTRIES.LIST GROUPBY="=trim(AR#)" ALTERNATETAGNAME="=if(len(trim(AK#))=0,&quot;ALLLEDGERENTRIES.LIST&quot;,&quot;&quot;)">
			<LEDGERNAME COLUMNREFERENCE="AR"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),&quot;Yes&quot;,&quot;No&quot;)"/>
			<AMOUNT FORMULA="={Round(AS#,2)}*if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),-1,1)"/>
		</LEDGERENTRIES.LIST>
		<!-- Ledger2 Before VAT -->
		<LEDGERENTRIES.LIST GROUPBY="=trim(AT#)" ALTERNATETAGNAME="=if(len(trim(AK#))=0,&quot;ALLLEDGERENTRIES.LIST&quot;,&quot;&quot;)">
			<LEDGERNAME COLUMNREFERENCE="AT"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),&quot;Yes&quot;,&quot;No&quot;)"/>
			<AMOUNT FORMULA="={Round(AU#,2)}*if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),-1,1)"/>
		</LEDGERENTRIES.LIST>
		<!-- Sales/Purchase Ledger -->
		<ALLLEDGERENTRIES.LIST GROUPBY="=trim(AY#)" SKIP="=Not(len(trim(AK#))=0)">
			<TAXCLASSIFICATIONNAME COLUMNREFERENCE="BE"/>
			<LEDGERNAME COLUMNREFERENCE="AY"/>
			<!--<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__(AV# &amp; &quot;-VATCLASS-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>-->
			<!--<LEDGERNAME FORMULA="=&quot;__LOOKUP__(AV# &amp; &quot;-LEDGER-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>-->
			<ISDEEMEDPOSITIVE FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),&quot;Yes&quot;,&quot;No&quot;)"/>
			<AMOUNT FORMULA="={Round(AQ#,2)}*if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),-1,1)"/>
		</ALLLEDGERENTRIES.LIST>
		<!-- VAT / CST Ledger -->
		<LEDGERENTRIES.LIST GROUPBY="=trim(AY#)" SKIP="={Round(BG#,2)}=0" ALTERNATETAGNAME="=if(len(trim(AK#))=0,&quot;ALLLEDGERENTRIES.LIST&quot;,&quot;&quot;)">
			<BASICRATEOFINVOICETAX.LIST TYPE="Number">
				<!--<BASICRATEOFINVOICETAX FORMULA="=&quot;__LOOKUP__(AV# &amp; &quot;-VATRATE-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>-->
				<BASICRATEOFINVOICETAX COLUMNREFERENCE="AW"/>
			</BASICRATEOFINVOICETAX.LIST>
			<TAXCLASSIFICATIONNAME COLUMNREFERENCE="BE"/>
			<LEDGERNAME COLUMNREFERENCE="AZ"/>
			<RATEOFADDLVAT COLUMNREFERENCE="AX"/>
			<!--<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__(AV# &amp; &quot;-VATCLASS-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>
			<LEDGERNAME FORMULA="=&quot;__LOOKUP__(AV# &amp; &quot;-VATLEDGER-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>
			<RATEOFADDLVAT FORMULA="=&quot;__LOOKUP__(AV# &amp; &quot;-ADDLVATRATE-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>-->
