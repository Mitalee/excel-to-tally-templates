<!-- These XML tags are added to Excel sheet Cell A1 at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" FROMROW="3" xmlns:UDF="TallyUDF">
	<!-- Specifies that the Column A is the key-field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>NAME</COLUMNNAME>
		<COLUMNNAME>ID</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!-- Create TARIFFCLASSIFICATION Masters -->
	<MASTER TYPE="TARIFFCLASSIFICATION">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="BH"/>
		</NAME.LIST>
		<!--<SCHEDULENUMBER>SchNo</SCHEDULENUMBER>
		<SCHEDULESERIALNUMBER>SchSrNo</SCHEDULESERIALNUMBER>-->
		<USEDFOR>VAT</USEDFOR>
		<UDF:COMMODITYVATRATE.LIST DESC="`CommodityVATRate`" ISLIST="YES" TYPE="Number" INDEX="10004">
			<UDF:COMMODITYVATRATE DESC="`CommodityVATRate`" COLUMNREFERENCE="BM"/>
		</UDF:COMMODITYVATRATE.LIST>
	</MASTER>
	<!-- Create TARIFFCLASSIFICATION Masters -->
	<MASTER TYPE="TARIFFCLASSIFICATION">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="BZ"/>
		</NAME.LIST>
		<USEDFOR>Both</USEDFOR>
	</MASTER>
	<!-- Create UNIT Masters -->
	<MASTER TYPE="UNIT">
		<!-- The UNIT Master does not support Aliases. -->
		<!-- As a result, we must not use the NAME.LIST tag, but directly use the NAME tag -->
		<NAME COLUMNREFERENCE="K"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create UNIT Masters -->
	<MASTER TYPE="UNIT">
		<!-- The UNIT Master does not support Aliases. -->
		<!-- As a result, we must not use the NAME.LIST tag, but directly use the NAME tag -->
		<NAME COLUMNREFERENCE="L"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create STOCKGROUP Masters -->
	<MASTER TYPE="STOCKGROUP">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="G"/>
		</NAME.LIST>
	</MASTER>
	<!-- Create STOCKCATEGORY Masters -->
	<MASTER TYPE="STOCKCATEGORY">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="H"/>
		</NAME.LIST>
	</MASTER>
	<!-- Create GODOWN Masters -->
	<MASTER TYPE="GODOWN">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="AG"/>
		</NAME.LIST>
	</MASTER>
	<!-- Create LEDGER Masters -->
	<!-- GST details will NOT be updated / set. It has to be done using the Ledger-Master template -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="W"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="X"/>
		<ISCOSTCENTREON>No</ISCOSTCENTREON>
		<!-- No for Duties and Taxes -->
		<AFFECTSSTOCK FORMULA="=if(trim(X#)=&quot;duties &amp; taxes&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
		<USEFORVAT FORMULA="=if(trim(X#)=&quot;duties &amp; taxes&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
		<TAXCLASSIFICATIONNAME COLUMNREFERENCE="Z"/>
	</MASTER>
  <!-- GST details will NOT be updated / set. It has to be done using the Ledger-Master template -->
  <MASTER TYPE="LEDGER">
    <NAME.LIST>
      <NAME COLUMNREFERENCE="AB"/>
    </NAME.LIST>
    <PARENT COLUMNREFERENCE="AC"/>
    <ISCOSTCENTREON>No</ISCOSTCENTREON>
    <!-- No for Duties and Taxes -->
    <AFFECTSSTOCK FORMULA="=if(trim(X#)=&quot;duties &amp; taxes&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
    <USEFORVAT FORMULA="=if(trim(X#)=&quot;duties &amp; taxes&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
    <TAXCLASSIFICATIONNAME COLUMNREFERENCE="AE"/>
  </MASTER>
	<!-- Create STOCKITEM Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST SKIP="=len(trim(D#))=0">
			<NAME COLUMNREFERENCE="B"/>
			<NAME SCROLL="YES" COLUMNREFERENCE="D"/>
		</NAME.LIST>
		<NAME SKIP="=Not(len(trim(D#))=0)" COLUMNREFERENCE="B"/>
		<OLDNAME COLUMNREFERENCE="C"/>
		<ADDITIONALNAME.LIST SKIP="=Or(len(trim(E#))=0,len(trim(F#))=0)">
			<ADDITIONALNAME COLUMNREFERENCE="E"/>
			<ADDITIONALNAME SCROLL="YES" COLUMNREFERENCE="F"/>
		</ADDITIONALNAME.LIST>
		<ADDITIONALNAME SKIP="=Not(len(trim(F#))=0)" COLUMNREFERENCE="E"/>
		<!-- StockGroup Name  -->
		<PARENT COLUMNREFERENCE="G"/>
		<!-- StockCategory Name  -->
		<CATEGORY COLUMNREFERENCE="H"/>
		<!-- Description -->
		<DESCRIPTION COLUMNREFERENCE="I"/>
		<!-- Remarks -->
		<NARRATION COLUMNREFERENCE="J"/>
		<!-- BaseUnits  -->
		<BASEUNITS COLUMNREFERENCE="K"/>
		<!-- Alternate Unit  -->
		<ADDITIONALUNITS FORMULA="=IF(len(trim(M#))=0,&quot;&quot;,L#)"/>
		<!-- Whether to Maintain in batches or not -->
		<!--<ISBATCHWISEON FORMULA="=IF(len(trim(N#))=0,&quot;No&quot;,N#)"/>-->
		<ISBATCHWISEON COLUMNREFERENCE="N"/>
		<!-- Whether to Track date of Mfg or not.  -->
		<!-- If set to Yes, then the Mfg.Date has to be given. -->
		<!--<HASMFGDATE FORMULA="=IF(len(trim(AI#))=0,&quot;No&quot;,O#)"/>-->
		<HASMFGDATE COLUMNREFERENCE="O"/>
		<!-- Whether to use Expiry dates or not.  -->
		<!-- If set to Yes, then the Expiry.Date has to be given. -->
		<!--<ISPERISHABLEON FORMULA="=IF(len(trim(AJ#))=0,&quot;No&quot;,P#)"/>-->
		<ISPERISHABLEON COLUMNREFERENCE="P"/>
		<!-- Standard Cost details -->
		<STANDARDCOSTLIST.LIST SCROLL="Yes" SKIP="=len(trim(Q#))=0" APPEND="=trim(DJ#)">
			<DATE COLUMNREFERENCE="Q"/>
			<RATE FORMULA="=R# &amp; if(len(trim(S#))=0,&quot;&quot;,&quot;/&quot; &amp; S#)"/>
		</STANDARDCOSTLIST.LIST>
		<!-- Standard Price details -->
		<STANDARDPRICELIST.LIST SCROLL="Yes" SKIP="=len(trim(T#))=0" APPEND="=trim(DK#)">
			<DATE COLUMNREFERENCE="T"/>
			<RATE FORMULA="=U# &amp; if(len(trim(V#))=0,&quot;&quot;,&quot;/&quot; &amp; V#)"/>
		</STANDARDPRICELIST.LIST>
		
		<!-- Default Sales Ledger Name and VAT Class -->
		<SALESLIST.LIST SCROLL="YES" SKIP="=len(trim(W#))=0">
			<NAME COLUMNREFERENCE="W"/>
      <!-- New Class for GST -->
      <GSTCLASSIFICATIONNATURE COLUMNREFERENCE="Y"/>
			<VATCLASSIFICATIONNATURE COLUMNREFERENCE="Z"/>
      <!-- Old VAT Class -->
			<TAXCLASSIFICATIONNAME COLUMNREFERENCE="Z"/>
			<CLASSRATE COLUMNREFERENCE="AA"/>
		</SALESLIST.LIST>
		
		<!-- Default Purchase Ledger Name and VAT Class -->
		<PURCHASELIST.LIST SCROLL="YES" SKIP="=len(trim(AB#))=0">
			<NAME COLUMNREFERENCE="AB"/>
      <!-- New Class for GST -->
      <GSTCLASSIFICATIONNATURE COLUMNREFERENCE="AD"/>
			<VATCLASSIFICATIONNATURE COLUMNREFERENCE="AE"/>
      <!-- Old VAT Class -->
			<TAXCLASSIFICATIONNAME COLUMNREFERENCE="AE"/>
			<CLASSRATE COLUMNREFERENCE="AF"/>
		</PURCHASELIST.LIST>

		<!-- Stock-item Opening Qty  -->
		<OPENINGBALANCE SKIP="=Or({Round(AK#,2)}=0,Not(Round(M#,2)=0))" FORMULA="={Round(AK#,2)}"/>
		<OPENINGBALANCE SKIP="Or({Round(AK#,2)}=0,Round(M#,2)=0)" FORMULA="={AK#} &amp; &quot; &quot; &amp; K# &amp; &quot; = &quot; &amp; Round({AK#}/M#,0) &amp; &quot; &quot; &amp; L#"/>
		<!-- Stock-item Rate -->
		<OPENINGRATE SKIP="={Round(AN#,2)}=0" FORMULA="=IF({Round(AK#,2)}&gt;0,Round({Round(AN#,2)}/{Round(AK#,2)},2) &amp; if(len(trim(AM#))=0,&quot;&quot;,&quot;/&quot; &amp; AM#),0)"/>
		<!-- Stock-item Value -->
		<OPENINGVALUE SKIP="={Round(AN#,2)}=0" FORMULA="={Round(AN#,2)} * -1"/>
		<!-- Batch wise Opening Stock details details-->
		<BATCHALLOCATIONS.LIST SCROLL="Yes" SKIP="=And(len(trim(AG#))=0,len(trim(AH#))=0)">
			<NAME COLUMNREFERENCE="AH"/>
			<BATCHNAME COLUMNREFERENCE="AH"/>
			<!-- If GodownName is not specified i.e. Column U value is blank, then the Godown Name must be Main Location else value of Column U-->
			<GODOWNNAME FORMULA="=IF(len(trim(AG#))=0,&quot;Main Location&quot;,+AG#)"/>
			<MFDON FORMULA="=IF(len(trim(AI#))=0,&quot;&quot;,AI#)"/>
			<EXPIRYPERIOD FORMULA="=IF(len(trim(AJ#))=0,&quot;&quot;,AJ#)"/>
			<!-- Stock-item Opening Qty  -->
			<OPENINGBALANCE SKIP="=Or(AK#=0,Not(Round(M#[1],2)=0))" COLUMNREFERENCE="AK"/>
			<OPENINGBALANCE SKIP="Or(AK#=0,Round(M#[1],2)=0)" FORMULA="=AK# &amp; &quot; &quot; &amp; K#[1] &amp; &quot; = &quot; &amp; Round(AK#/M#[1],0) &amp; &quot; &quot; &amp; L#[1]"/>
			<!-- Stock-item Rate -->
			<OPENINGRATE SKIP="=Round(AL#,2)=0" FORMULA="=AL# &amp; if(len(trim(AM#))=0,&quot;&quot;,&quot;/&quot; &amp; AM#)"/>
			<!-- Stock-item Value -->
			<OPENINGVALUE SKIP="=Round(AN#,2)=0" FORMULA="=Round(AN#,2) * -1"/>
		</BATCHALLOCATIONS.LIST>

		<!-- GST details -->
    <GSTAPPLICABLE SKIP="=And(len(trim(AO#))=0,len(trim(AP#))=0,len(trim(AQ#))=0,len(trim(AR#))=0,len(trim(BE#))=0,len(trim(AW#))=0,len(trim(AZ#))=0,len(trim(BA#))=0)" FORMULA="=if(len(trim(AO#))=0,&quot;~[~ Applicable&quot;,&quot;~[~ &quot; &amp; AO#)"/>

		<!-- Type of supply -->
    <GSTTYPEOFSUPPLY FORMULA="=if(len(trim(BE#))=0,&quot;Goods&quot;,BE#)"/>

		<!-- Empty GSTDETAILS tag not required as it will overwrite existing data, if any  -->
		<GSTDETAILS.LIST SKIP="=And(len(trim(AQ#))=0,len(trim(AR#))=0,len(trim(BE#))=0,len(trim(AW#))=0,len(trim(AZ#))=0)" APPEND="=trim(DI#)">
      <APPLICABLEFROM FORMULA="=if(len(trim(AP#))=0,&quot;20170701&quot;,AP#)"/>
      <CALCULATIONTYPE FORMULA="=if(len(trim(AT#))=0,&quot;On Value&quot;,AT#)"/>

			<HSNCODE COLUMNREFERENCE="AQ"/>
			<HSNMASTERNAME COLUMNREFERENCE="BF"/>
			<HSN COLUMNREFERENCE="AR"/>
      <TAXABILITY SKIP="=trim(AT#[1])=&quot;On Item Rate&quot;" FORMULA="=if(len(trim(AW#))=0,&quot;Taxable&quot;,AW#)"/>

			<ISREVERSECHARGEAPPLICABLE COLUMNREFERENCE="AX"/>
			<ISNONGSTGOODS COLUMNREFERENCE="AS"/>
			<GSTINELIGIBLEITC COLUMNREFERENCE="AY"/>
			<STATEWISEDETAILS.LIST>
				<STATENAME>~[~ Any</STATENAME>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(BA#,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD FORMULA="=if(Round(BC#,2)&gt;0,&quot;UT Tax&quot;,&quot;State Tax&quot;)"/>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=if(Round(BC#,2)&gt;0,Round(BC#,2),Round(BB#,2))"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AZ#,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(BD#,2)"/>
				</RATEDETAILS.LIST>
				<!-- When CALCULATION-TYPE is set as : On Item Rate -->
				<GSTSLABRATES.LIST SKIP="=Or(len(trim(AT#[1]))=0,Not(trim(AT#[1])=&quot;On Item Rate&quot;))" SCROLL="YES">
					<TAXABILITY COLUMNREFERENCE="AW"/>
					<TOITEMRATE FORMULA="=Round(AV#,2)"/>
					<RATEDETAILS.LIST>
						<GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(BA#,2)"/>
					</RATEDETAILS.LIST>
					<RATEDETAILS.LIST>
						<!-- <GSTRATEDUTYHEAD>State Tax</GSTRATEDUTYHEAD> -->
						<GSTRATEDUTYHEAD FORMULA="=if(Round(BC#,2)&gt;0,&quot;UT Tax&quot;,&quot;State Tax&quot;)"/>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=if(Round(BC#,2)&gt;0,Round(BC#,2),Round(BB#,2))"/>
					</RATEDETAILS.LIST>
					<RATEDETAILS.LIST>
						<GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(AZ#,2)"/>
					</RATEDETAILS.LIST>
					<RATEDETAILS.LIST>
						<GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(BD#,2)"/>
					</RATEDETAILS.LIST>
				</GSTSLABRATES.LIST>
			</STATEWISEDETAILS.LIST>
			<TEMPGSTDETAILSLABRATES.LIST>
			</TEMPGSTDETAILSLABRATES.LIST>
		</GSTDETAILS.LIST>

		<!-- VAT details -->
		<VATAPPLICABLE SKIP="=len(trim(BG#))=0" FORMULA="=&quot;~[~ &quot; &amp; trim(BG#)"/>
		<!-- VAT Commodity -->
		<VATCOMMODITY COLUMNREFERENCE="BH"/>
		<!-- Rate of VAT-->
		<RATEOFVAT COLUMNREFERENCE="BM"/>
		<VATBASENO COLUMNREFERENCE="BS"/>
		<VATBASEUNIT COLUMNREFERENCE="BT"/>
		<VATTRAILNO COLUMNREFERENCE="BU"/>
		<VATTRAILUNIT COLUMNREFERENCE="BV"/>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<VATDETAILS.LIST SKIP="=Not(Or(trim(BG#)=&quot;applicable&quot;,trim(BG#)=&quot;yes&quot;))">
			<FROMDATE COLUMNREFERENCE="BO"/>
			<TAXTYPE FORMULA="=if(Len(trim(BP#))=0,&quot;Taxable&quot;,BP#)"/>
			<GOODSNATURE COLUMNREFERENCE="BI"/>
			<VATCOMMODITYNAME COLUMNREFERENCE="BJ"/>
			<VATCOMMODITYCODE COLUMNREFERENCE="BK"/>
			<REPORTINGUOM COLUMNREFERENCE="BL"/>
			<ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
			<RATEOFVAT COLUMNREFERENCE="BM"/>
			<ADDITIONALTAX COLUMNREFERENCE="BN"/>
		</VATDETAILS.LIST>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<INCLUSIVETAX COLUMNREFERENCE="BW"/>
		<!-- Calc Tax on MRP / MRP to print -->
		<CALCONMRP COLUMNREFERENCE="BQ"/>
		<!-- MRP Incl of VAT -->
		<ISMRPINCLOFTAX COLUMNREFERENCE="BR"/>

		<!-- Excise details -->
		<EXCISEAPPLICABILITY SKIP="=len(trim(BX#))=0" FORMULA="=&quot;~[~ &quot; &amp; trim(BX#)"/>
		<!-- Tariff Classification -->
		<BASICTARIFFTYPE COLUMNREFERENCE="BZ"/>
		<!-- Rate of Excise Duty -->
		<BASICRATEOFEXCISE COLUMNREFERENCE="CN"/>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<EXCISETARIFFDETAILS.LIST SKIP="=len(trim(CN#))=0">
			<APPLICABLEFROM COLUMNREFERENCE="CQ"/>
			<TYPEOFTARIFF FORMULA="=if(len(trim(CC#))=0,&quot;Dutiable&quot;,CC#)"/>
			<EXCISECLASSIFICATIONNAME COLUMNREFERENCE="BY"/>
			<STOCKITEMTYPE COLUMNREFERENCE="CE"/>
			<ISEXCISECALCULATEONMRP COLUMNREFERENCE="CJ"/>
			<ISNONDUTIABLE>No</ISNONDUTIABLE>
			<ABATEMENTPERCENTAGE COLUMNREFERENCE="CK"/>
			<REPORTINGUOM COLUMNREFERENCE="CB"/>
			<TARIFFNAME COLUMNREFERENCE="BZ"/>
			<VALUATIONTYPE FORMULA="=if(len(trim(CM#))=0,&quot;Ad Valorem&quot;,CM#)"/>
			<HSNCODE COLUMNREFERENCE="CA"/>
			<EXCISEDUTYHEADDETAILS.LIST SCROLL="YES">
				<VALUATIONTYPE FORMULA="=if(len(trim(CM#))=0,&quot;Ad Valorem&quot;,CM#)"/>
				<STOCKITEMTYPE COLUMNREFERENCE="CE"/>
				<DUTYHEAD FORMULA="=if(len(trim(CL#))=0,&quot;Basic Excise Duty&quot;,CL#)"/>
				<EXCISERATE COLUMNREFERENCE="CN"/>
				<NOTIFICATIONNUMBER COLUMNREFERENCE="CO"/>
				<NOTIFICATIONSERIAL COLUMNREFERENCE="CP"/>
			</EXCISEDUTYHEADDETAILS.LIST>
		</EXCISETARIFFDETAILS.LIST>
		<EXCISEITEMGODOWN.LIST SKIP="=len(trim(CE#))=0">
			<!-- The TaxUnitName master must exist in Tally  -->
			<TAXUNITNAME COLUMNREFERENCE="CD"/>
			<!-- If StockItemType is specified, then the TaxUnitName must also be specified -->
			<STOCKITEMTYPE COLUMNREFERENCE="CE"/>
			<!-- 
			Ex: 1 Pcs = 32 sq feet / sq meter
			where 
			BASEUNIT =  sq feet; 
			ALTERNATEUNIT = Pcs
			-->
			<!-- 1 PCs -->
			<EXCISEREPDENOMINATOR COLUMNREFERENCE="CF"/>
			<EXCISECONVUNIT COLUMNREFERENCE="CG"/>
			<!-- 32 sq feet -->
			<EXCISEALTREPUNITS COLUMNREFERENCE="CH"/>
			<EXCISEREPUNITSSTR COLUMNREFERENCE="CI"/>
		</EXCISEITEMGODOWN.LIST>

		<!-- MRP -->
		<RATEOFMRP COLUMNREFERENCE="CS"/>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<MRPDETAILS.LIST SKIP="=len(trim(CS#))=0" APPEND="=trim(DL#)">
			<FROMDATE COLUMNREFERENCE="CR"/>
			<MRPRATEDETAILS.LIST>
				<MRPRATE COLUMNREFERENCE="CS"/>
			</MRPRATEDETAILS.LIST>
		</MRPDETAILS.LIST>

		<!-- Valuation Behaviour -->
		<COSTINGMETHOD COLUMNREFERENCE="CT"/>
		<VALUATIONMETHOD COLUMNREFERENCE="CU"/>
		<!-- Behaviour -->
		<IGNOREPHYSICALDIFFERENCE COLUMNREFERENCE="CV"/>
		<IGNORENEGATIVESTOCK COLUMNREFERENCE="CW"/>
		<TREATSALESASMANUFACTURED COLUMNREFERENCE="CX"/>
		<TREATPURCHASESASCONSUMED COLUMNREFERENCE="CY"/>
		<TREATREJECTSASSCRAP COLUMNREFERENCE="CZ"/>
		<!-- REORDER DETAILS -->
		<REORDERBASE COLUMNREFERENCE="DA"/>
		<REORDERPERIODLENGTH COLUMNREFERENCE="DB"/>
		<REORDERPERIOD COLUMNREFERENCE="DC"/>
		<!-- Reorder Level -->
		<REORDERASHIGHER SKIP="=len(trim(DD#))=0" FORMULA="=If(DD#=&quot;Higher&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
		<!-- MINIMUM REORDER DETAILS -->
		<MINIMUMORDERBASE COLUMNREFERENCE="DE"/>
		<MINORDERPERIODLENGTH COLUMNREFERENCE="DF"/>
		<MINORDERPERIOD COLUMNREFERENCE="DG"/>
		<MINORDERASHIGHER SKIP="=len(trim(DH#))=0" FORMULA="=If(DH#=&quot;Higher&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
		<!-- Conversion factor details-->
		<DENOMINATOR FORMULA="=IF(len(trim(L#))=0,&quot;&quot;,M#)"/>
		<CONVERSION FORMULA="=IF(len(trim(L#))=0,&quot;&quot;,1)"/>
	</MASTER>
</XMLTAGS>