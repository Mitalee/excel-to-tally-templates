<!-- These XML tags are added to Excel sheet Cell A1 at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that the Column A is the key-field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>NAME</COLUMNNAME>
		<COLUMNNAME>ID</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!-- Create TARIFFCLASSIFICATION Masters -->
	<MASTER TYPE="TARIFFCLASSIFICATION">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="BA"/>
		</NAME.LIST>
		<!--<SCHEDULENUMBER>SchNo</SCHEDULENUMBER>
		<SCHEDULESERIALNUMBER>SchSrNo</SCHEDULESERIALNUMBER>-->
		<USEDFOR>VAT</USEDFOR>
		<UDF:COMMODITYVATRATE.LIST DESC="`CommodityVATRate`" ISLIST="YES" TYPE="Number" INDEX="10004">
			<UDF:COMMODITYVATRATE DESC="`CommodityVATRate`" COLUMNREFERENCE="BF"/>
		</UDF:COMMODITYVATRATE.LIST>
	</MASTER>
	<!-- Create TARIFFCLASSIFICATION Masters -->
	<MASTER TYPE="TARIFFCLASSIFICATION">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="BS"/>
		</NAME.LIST>
		<USEDFOR>Both</USEDFOR>
	</MASTER>
	<!-- Create UNIT Masters -->
	<MASTER TYPE="UNIT">
		<!-- The UNIT Master does not support Aliases. -->
		<!-- As a result, we must not use the NAME.LIST tag, but directly use the NAME tag -->
		<NAME COLUMNREFERENCE="K"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create UNIT Masters -->
	<MASTER TYPE="UNIT">
		<!-- The UNIT Master does not support Aliases. -->
		<!-- As a result, we must not use the NAME.LIST tag, but directly use the NAME tag -->
		<NAME COLUMNREFERENCE="L"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create STOCKGROUP Masters -->
	<MASTER TYPE="STOCKGROUP">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="G"/>
		</NAME.LIST>
	</MASTER>
	<!-- Create STOCKCATEGORY Masters -->
	<MASTER TYPE="STOCKCATEGORY">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="H"/>
		</NAME.LIST>
	</MASTER>
	<!-- Create GODOWN Masters -->
	<MASTER TYPE="GODOWN">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="AC"/>
		</NAME.LIST>
	</MASTER>
	<!-- Create LEDGER Masters -->
	<!-- GST details will NOT be updated / set. It has to be done using the Ledger-Master template -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="X"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="Y"/>
		<ISCOSTCENTREON>No</ISCOSTCENTREON>
		<!-- No for Duties and Taxes -->
		<AFFECTSSTOCK FORMULA="=if(trim(Y#)=&quot;duties &amp; taxes&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
		<USEFORVAT FORMULA="=if(trim(Y#)=&quot;duties &amp; taxes&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
		<TAXCLASSIFICATIONNAME COLUMNREFERENCE="AA"/>
	</MASTER>
	<!-- Create STOCKITEM Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST SKIP="=len(trim(D#))=0">
			<NAME COLUMNREFERENCE="B"/>
			<NAME SCROLL="YES" COLUMNREFERENCE="D"/>
		</NAME.LIST>
		<NAME SKIP="=Not(len(trim(D#))=0)" COLUMNREFERENCE="B"/>
		<OLDNAME COLUMNREFERENCE="C" />
		<ADDITIONALNAME.LIST SKIP="=Or(len(trim(E#))=0,len(trim(F#))=0)">
			<ADDITIONALNAME COLUMNREFERENCE="E"/>
			<ADDITIONALNAME SCROLL="YES" COLUMNREFERENCE="F"/>
		</ADDITIONALNAME.LIST>
		<ADDITIONALNAME SKIP="=Not(len(trim(F#))=0)" COLUMNREFERENCE="E"/>
		<!-- StockGroup Name  -->
		<PARENT COLUMNREFERENCE="G"/>
		<!-- StockCategory Name  -->
		<CATEGORY COLUMNREFERENCE="H"/>
		<!-- Description -->
		<DESCRIPTION COLUMNREFERENCE="I"/>
		<!-- Remarks -->
		<NARRATION COLUMNREFERENCE="J"/>
		<!-- BaseUnits  -->
		<BASEUNITS COLUMNREFERENCE="K"/>
		<!-- Alternate Unit  -->
		<ADDITIONALUNITS FORMULA="=IF(len(trim(M#))=0,&quot;&quot;,L#)"/>
		<!-- Whether to Maintain in batches or not -->
		<!--<ISBATCHWISEON FORMULA="=IF(len(trim(N#))=0,&quot;No&quot;,N#)"/>-->
		<ISBATCHWISEON COLUMNREFERENCE="N"/>
		<!-- Whether to Track date of Mfg or not.  -->
		<!-- If set to Yes, then the Mfg.Date has to be given. -->
		<!--<HASMFGDATE FORMULA="=IF(len(trim(AE#))=0,&quot;No&quot;,O#)"/>-->
		<HASMFGDATE COLUMNREFERENCE="O"/>
		<!-- Whether to use Expiry dates or not.  -->
		<!-- If set to Yes, then the Expiry.Date has to be given. -->
		<!--<ISPERISHABLEON FORMULA="=IF(len(trim(AF#))=0,&quot;No&quot;,P#)"/>-->
		<ISPERISHABLEON COLUMNREFERENCE="P"/>
		<!-- Standard Cost details -->
		<STANDARDCOSTLIST.LIST SCROLL="Yes" SKIP="=len(trim(Q#))=0" APPEND="No">
			<DATE COLUMNREFERENCE="Q"/>
			<RATE FORMULA="=R# &amp; &quot;/&quot; &amp; +S#"/>
		</STANDARDCOSTLIST.LIST>
		<!-- Standard Price details -->
		<STANDARDPRICELIST.LIST SCROLL="Yes" SKIP="=len(trim(T#))=0" APPEND="No">
			<DATE COLUMNREFERENCE="T"/>
			<RATE FORMULA="=U# &amp; &quot;/&quot; &amp; +V#"/>
		</STANDARDPRICELIST.LIST>
		<!-- Default Sales Ledger Name and VAT Class -->
		<SALESLIST.LIST SCROLL="YES" SKIP="=not(trim(W#)=&quot;sales&quot;)">
			<NAME COLUMNREFERENCE="X"/>
			<VATCLASSIFICATIONNATURE COLUMNREFERENCE="Z"/>
			<TAXCLASSIFICATIONNAME COLUMNREFERENCE="AA"/>
			<CLASSRATE COLUMNREFERENCE="AB"/>
		</SALESLIST.LIST>
		<!-- Default Purchase Ledger Name and VAT Class -->
		<PURCHASELIST.LIST SCROLL="YES" SKIP="=not(trim(W#)=&quot;purchase&quot;)">
			<NAME COLUMNREFERENCE="X"/>
			<VATCLASSIFICATIONNATURE COLUMNREFERENCE="Z"/>
			<TAXCLASSIFICATIONNAME COLUMNREFERENCE="AA"/>
			<CLASSRATE COLUMNREFERENCE="AB"/>
		</PURCHASELIST.LIST>
		<!-- Stock-item Opening Qty  -->
		<OPENINGBALANCE SKIP="=Or({Round(AG#,2)}=0,Not(Round(M#,2)=0))" FORMULA="={Round(AG#,2)}"/>
		<OPENINGBALANCE SKIP="Or({Round(AG#,2)}=0,Round(M#,2)=0)" FORMULA="={AG#} &amp; &quot; &quot; &amp; K# &amp; &quot; = &quot; &amp; Round({AG#}/M#,0) &amp; &quot; &quot; &amp; L#"/>
		<!-- Stock-item Rate -->
		<OPENINGRATE SKIP="={Round(AJ#,2)}=0" FORMULA="=IF({Round(AG#,2)}&gt;0,Round({Round(AJ#,2)}/{Round(AG#,2)},2) &amp; &quot;/&quot; &amp; AI#,0)"/>
		<!-- Stock-item Value -->
		<OPENINGVALUE SKIP="={Round(AJ#,2)}=0" FORMULA="={Round(AJ#,2)} * -1"/>
		<!-- Batch wise Opening Stock details details-->
		<BATCHALLOCATIONS.LIST SCROLL="Yes" SKIP="=And(len(trim(AC#))=0,len(trim(AD#))=0)">
			<NAME COLUMNREFERENCE="AD"/>
			<BATCHNAME COLUMNREFERENCE="AD"/>
			<!-- If GodownName is not specified i.e. Column U value is blank, then the Godown Name must be Main Location else value of Column U-->
			<GODOWNNAME FORMULA="=IF(len(trim(AC#))=0,&quot;Main Location&quot;,+AC#)"/>
			<MFDON FORMULA="=IF(len(trim(AE#))=0,&quot;&quot;,AE#)"/>
			<EXPIRYPERIOD FORMULA="=IF(len(trim(AF#))=0,&quot;&quot;,AF#)"/>
			<!-- Stock-item Opening Qty  -->
			<OPENINGBALANCE SKIP="=Or(AG#=0,Not(Round(M#[1],2)=0))" COLUMNREFERENCE="AG"/>
			<OPENINGBALANCE SKIP="Or(AG#=0,Round(M#[1],2)=0)" FORMULA="=AG# &amp; &quot; &quot; &amp; K#[1] &amp; &quot; = &quot; &amp; Round(AG#/M#[1],0) &amp; &quot; &quot; &amp; L#[1]"/>
			<!-- Stock-item Rate -->
			<OPENINGRATE SKIP="=Round(AH#,2)=0" FORMULA="=AH# &amp; &quot;/&quot; &amp; AI#"/>
			<!-- Stock-item Value -->
			<OPENINGVALUE SKIP="=Round(AJ#,2)=0" FORMULA="=Round(AJ#,2) * -1"/>
		</BATCHALLOCATIONS.LIST>
		<!-- GST details -->
		<GSTAPPLICABLE SKIP="=len(trim(AK#))=0" FORMULA="=&quot;~[~ &quot; &amp; AK#"/>
		<!-- Type of supply -->
		<GSTTYPEOFSUPPLY COLUMNREFERENCE="AX"/>
		<!-- Empty GSTDETAILS tag not required as it will overwrite existing data, if any  -->
		<GSTDETAILS.LIST SKIP="=And(len(trim(AM#))=0,len(trim(AN#))=0,len(trim(AX#))=0,len(trim(AS#))=0,len(trim(AV#))=0)">
			<APPLICABLEFROM COLUMNREFERENCE="AL"/>
			<CALCULATIONTYPE COLUMNREFERENCE="AP"/>
			<HSNCODE COLUMNREFERENCE="AM"/>
			<HSNMASTERNAME COLUMNREFERENCE="AY"/>
			<HSN COLUMNREFERENCE="AN"/>
			<TAXABILITY SKIP="=Or(len(trim(AP#[1]))=0,trim(AP#[1])=&quot;On Item Rate&quot;)" COLUMNREFERENCE="AS"/>
			<ISREVERSECHARGEAPPLICABLE COLUMNREFERENCE="AT"/>
			<ISNONGSTGOODS COLUMNREFERENCE="AO"/>
			<GSTINELIGIBLEITC COLUMNREFERENCE="AU"/>
			<STATEWISEDETAILS.LIST>
				<STATENAME>~[~ Any</STATENAME>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AV#/2,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>State Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AV#/2,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AV#,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AW#,2)"/>
				</RATEDETAILS.LIST>
				<!-- When CALCULATION-TYPE is set as : On Item Rate -->
				<GSTSLABRATES.LIST SKIP="=Or(len(trim(AP#[1]))=0,Not(trim(AP#[1])=&quot;On Item Rate&quot;))" SCROLL="YES">
					<TAXABILITY COLUMNREFERENCE="AS"/>
					<TOITEMRATE FORMULA="=Round(AR#,2)"/>
					<RATEDETAILS.LIST>
						<GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(AV#/2,2)"/>
					</RATEDETAILS.LIST>
					<RATEDETAILS.LIST>
						<GSTRATEDUTYHEAD>State Tax</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(AV#/2,2)"/>
					</RATEDETAILS.LIST>
					<RATEDETAILS.LIST>
						<GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(AV#,2)"/>
					</RATEDETAILS.LIST>
					<RATEDETAILS.LIST>
						<GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(AW#,2)"/>
					</RATEDETAILS.LIST>
				</GSTSLABRATES.LIST>
			</STATEWISEDETAILS.LIST>
			<TEMPGSTDETAILSLABRATES.LIST>
			</TEMPGSTDETAILSLABRATES.LIST>
		</GSTDETAILS.LIST>
		<!-- VAT Commodity -->
		<VATCOMMODITY COLUMNREFERENCE="BA"/>
		<!-- Rate of VAT-->
		<RATEOFVAT COLUMNREFERENCE="BF"/>
		<VATBASENO COLUMNREFERENCE="BL"/>
		<VATBASEUNIT COLUMNREFERENCE="BM"/>
		<VATTRAILNO COLUMNREFERENCE="BN"/>
		<VATTRAILUNIT COLUMNREFERENCE="BO"/>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<VATDETAILS.LIST SKIP="=Not(Or(trim(AZ#)=&quot;applicable&quot;,trim(AZ#)=&quot;yes&quot;))">
			<FROMDATE COLUMNREFERENCE="BH"/>
			<TAXTYPE FORMULA="=if(Len(trim(BI#))=0,&quot;Taxable&quot;,BI#)"/>
			<GOODSNATURE COLUMNREFERENCE="BB"/>
			<VATCOMMODITYNAME COLUMNREFERENCE="BC"/>
			<VATCOMMODITYCODE COLUMNREFERENCE="BD"/>
			<REPORTINGUOM COLUMNREFERENCE="BE"/>
			<ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
			<RATEOFVAT COLUMNREFERENCE="BF"/>
			<ADDITIONALTAX COLUMNREFERENCE="BG"/>
		</VATDETAILS.LIST>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<INCLUSIVETAX COLUMNREFERENCE="BP"/>
		<!-- <VATAPPLICABLE SKIP="=Round(BF#,2)=0">~[~ Applicable</VATAPPLICABLE> -->
		<VATAPPLICABLE SKIP="=len(trim(AZ#))=0" FORMULA="=&quot;~[~ &quot; &amp; trim(AZ#)"/>
		<!-- <EXCISEAPPLICABILITY SKIP="=And(Round(CG#,2)=0,len(trim(BT#))=0)">~[~ Applicable</EXCISEAPPLICABILITY> -->
		<EXCISEAPPLICABILITY SKIP="=len(trim(BQ#))=0" FORMULA="=&quot;~[~ &quot; &amp; trim(BQ#)"/>
		<!-- Tariff Classification -->
		<BASICTARIFFTYPE COLUMNREFERENCE="BS"/>
		<!-- Rate of Excise Duty -->
		<BASICRATEOFEXCISE COLUMNREFERENCE="CG"/>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<EXCISETARIFFDETAILS.LIST SKIP="=len(trim(CG#))=0">
			<APPLICABLEFROM COLUMNREFERENCE="CJ"/>
			<TYPEOFTARIFF FORMULA="=if(len(trim(BV#))=0,&quot;Dutiable&quot;,BV#)"/>
			<EXCISECLASSIFICATIONNAME COLUMNREFERENCE="BR"/>
			<STOCKITEMTYPE COLUMNREFERENCE="BX"/>
			<ISEXCISECALCULATEONMRP COLUMNREFERENCE="CC"/>
			<ISNONDUTIABLE>No</ISNONDUTIABLE>
			<ABATEMENTPERCENTAGE COLUMNREFERENCE="CD"/>
			<REPORTINGUOM COLUMNREFERENCE="BU"/>
			<TARIFFNAME COLUMNREFERENCE="BS"/>
			<VALUATIONTYPE FORMULA="=if(len(trim(CF#))=0,&quot;Ad Valorem&quot;,CF#)"/>
			<HSNCODE COLUMNREFERENCE="BT"/>
			<EXCISEDUTYHEADDETAILS.LIST SCROLL="YES">
				<VALUATIONTYPE FORMULA="=if(len(trim(CF#))=0,&quot;Ad Valorem&quot;,CF#)"/>
				<STOCKITEMTYPE COLUMNREFERENCE="BX"/>
				<DUTYHEAD FORMULA="=if(len(trim(CE#))=0,&quot;Basic Excise Duty&quot;,CE#)"/>
				<EXCISERATE COLUMNREFERENCE="CG"/>
				<NOTIFICATIONNUMBER COLUMNREFERENCE="CH"/>
				<NOTIFICATIONSERIAL COLUMNREFERENCE="CI"/>
			</EXCISEDUTYHEADDETAILS.LIST>
		</EXCISETARIFFDETAILS.LIST>
		<EXCISEITEMGODOWN.LIST SKIP="=len(trim(BX#))=0">
			<!-- The TaxUnitName master must exist in Tally  -->
			<TAXUNITNAME COLUMNREFERENCE="BW"/>
			<!-- If StockItemType is specified, then the TaxUnitName must also be specified -->
			<STOCKITEMTYPE COLUMNREFERENCE="BX"/>
			<!-- 
			Ex: 1 Pcs = 32 sq feet / sq meter
			where 
			BASEUNIT =  sq feet; 
			ALTERNATEUNIT = Pcs
			-->
			<!-- 1 PCs -->
			<EXCISEREPDENOMINATOR COLUMNREFERENCE="BY"/>
			<EXCISECONVUNIT COLUMNREFERENCE="BZ"/>
			<!-- 32 sq feet -->
			<EXCISEALTREPUNITS COLUMNREFERENCE="CA"/>
			<EXCISEREPUNITSSTR COLUMNREFERENCE="CB"/>
		</EXCISEITEMGODOWN.LIST>
		<!-- MRP -->
		<RATEOFMRP COLUMNREFERENCE="CK"/>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<MRPDETAILS.LIST SKIP="=len(trim(CK#))=0">
			<FROMDATE FORMULA="=(&quot;20160401&quot;)"/>
			<MRPRATEDETAILS.LIST>
				<MRPRATE COLUMNREFERENCE="CK"/>
			</MRPRATEDETAILS.LIST>
		</MRPDETAILS.LIST>
		<!-- Calc Tax on MRP / MRP to print -->
		<CALCONMRP COLUMNREFERENCE="BJ"/>
		<!-- MRP Incl of VAT -->
		<ISMRPINCLOFTAX COLUMNREFERENCE="BK"/>
		<!-- Behaviour -->
		<COSTINGMETHOD COLUMNREFERENCE="CL"/>
		<VALUATIONMETHOD COLUMNREFERENCE="CM"/>
		<IGNOREPHYSICALDIFFERENCE COLUMNREFERENCE="CN"/>
		<IGNORENEGATIVESTOCK COLUMNREFERENCE="CO"/>
		<TREATSALESASMANUFACTURED COLUMNREFERENCE="CP"/>
		<TREATPURCHASESASCONSUMED COLUMNREFERENCE="CQ"/>
		<TREATREJECTSASSCRAP COLUMNREFERENCE="CR"/>
		<!-- REORDER DETAILS -->
		<REORDERBASE COLUMNREFERENCE="CS"/>
		<REORDERPERIODLENGTH COLUMNREFERENCE="CT"/>
		<REORDERPERIOD COLUMNREFERENCE="CU"/>
		<!-- Reorder Level -->
		<REORDERASHIGHER SKIP="=len(trim(CV#))=0" FORMULA="=If(CV#=&quot;Higher&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
		<!-- MINIMUM REORDER DETAILS -->
		<MINIMUMORDERBASE COLUMNREFERENCE="CW"/>
		<MINORDERPERIODLENGTH COLUMNREFERENCE="CX"/>
		<MINORDERPERIOD COLUMNREFERENCE="CY"/>
		<MINORDERASHIGHER SKIP="=len(trim(CZ#))=0" FORMULA="=If(CZ#=&quot;Higher&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
		<!-- Conversion factor details-->
		<DENOMINATOR FORMULA="=IF(len(trim(L#))=0,&quot;&quot;,M#)"/>
		<CONVERSION FORMULA="=IF(len(trim(L#))=0,&quot;&quot;,1)"/>
	</MASTER>
</XMLTAGS>

