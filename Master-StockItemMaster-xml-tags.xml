<!-- These XML tags are added to Excel sheet Cell A1 at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that the Column A is the key-field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>NAME</COLUMNNAME>
		<COLUMNNAME>ID</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!-- Create TARIFFCLASSIFICATION Masters -->
	<MASTER TYPE="TARIFFCLASSIFICATION">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="AZ"/>
		</NAME.LIST>
		<!--<SCHEDULENUMBER>SchNo</SCHEDULENUMBER>
		<SCHEDULESERIALNUMBER>SchSrNo</SCHEDULESERIALNUMBER>-->
		<USEDFOR>VAT</USEDFOR>
		<UDF:COMMODITYVATRATE.LIST DESC="`CommodityVATRate`" ISLIST="YES" TYPE="Number" INDEX="10004">
			<UDF:COMMODITYVATRATE DESC="`CommodityVATRate`" COLUMNREFERENCE="BE"/>
		</UDF:COMMODITYVATRATE.LIST>
	</MASTER>
	<!-- Create TARIFFCLASSIFICATION Masters -->
	<MASTER TYPE="TARIFFCLASSIFICATION">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="BR"/>
		</NAME.LIST>
		<USEDFOR>Both</USEDFOR>
	</MASTER>
	<!-- Create UNIT Masters -->
	<MASTER TYPE="UNIT">
		<!-- The UNIT Master does not support Aliases. -->
		<!-- As a result, we must not use the NAME.LIST tag, but directly use the NAME tag -->
		<NAME COLUMNREFERENCE="J"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create UNIT Masters -->
	<MASTER TYPE="UNIT">
		<!-- The UNIT Master does not support Aliases. -->
		<!-- As a result, we must not use the NAME.LIST tag, but directly use the NAME tag -->
		<NAME COLUMNREFERENCE="K"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create STOCKGROUP Masters -->
	<MASTER TYPE="STOCKGROUP">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="F"/>
		</NAME.LIST>
	</MASTER>
	<!-- Create STOCKCATEGORY Masters -->
	<MASTER TYPE="STOCKCATEGORY">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="G"/>
		</NAME.LIST>
	</MASTER>
	<!-- Create GODOWN Masters -->
	<MASTER TYPE="GODOWN">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="AB"/>
		</NAME.LIST>
	</MASTER>
	<!-- Create LEDGER Masters -->
	<!-- GST details will NOT be updated / set. It has to be done using the Ledger-Master template -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="W"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="X"/>
		<ISCOSTCENTREON>No</ISCOSTCENTREON>
		<!-- No for Duties and Taxes -->
		<AFFECTSSTOCK FORMULA="=if(trim(X#)=&quot;duties &amp; taxes&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
		<USEFORVAT FORMULA="=if(trim(X#)=&quot;duties &amp; taxes&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
		<TAXCLASSIFICATIONNAME COLUMNREFERENCE="Z"/>
	</MASTER>
	<!-- Create STOCKITEM Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST SKIP="=len(trim(C#))=0">
			<NAME COLUMNREFERENCE="B"/>
			<NAME SCROLL="YES" COLUMNREFERENCE="C"/>
		</NAME.LIST>
		<NAME SKIP="=Not(len(trim(C#))=0)" COLUMNREFERENCE="B"/>
		<ADDITIONALNAME.LIST SKIP="=Or(len(trim(D#))=0,len(trim(E#))=0)">
			<ADDITIONALNAME COLUMNREFERENCE="D"/>
			<ADDITIONALNAME SCROLL="YES" COLUMNREFERENCE="E"/>
		</ADDITIONALNAME.LIST>
		<ADDITIONALNAME SKIP="=Not(len(trim(E#))=0)" COLUMNREFERENCE="D"/>
		<!-- StockGroup Name  -->
		<PARENT COLUMNREFERENCE="F"/>
		<!-- StockCategory Name  -->
		<CATEGORY COLUMNREFERENCE="G"/>
		<!-- Description -->
		<DESCRIPTION COLUMNREFERENCE="H"/>
		<!-- Remarks -->
		<NARRATION COLUMNREFERENCE="I"/>
		<!-- BaseUnits  -->
		<BASEUNITS COLUMNREFERENCE="J"/>
		<!-- Alternate Unit  -->
		<ADDITIONALUNITS FORMULA="=IF(len(trim(L#))=0,&quot;&quot;,K#)"/>
		<!-- Whether to Maintain in batches or not -->
		<!--<ISBATCHWISEON FORMULA="=IF(len(trim(M#))=0,&quot;No&quot;,M#)"/>-->
		<ISBATCHWISEON COLUMNREFERENCE="M"/>
		<!-- Whether to Track date of Mfg or not.  -->
		<!-- If set to Yes, then the Mfg.Date has to be given. -->
		<!--<HASMFGDATE FORMULA="=IF(len(trim(AD#))=0,&quot;No&quot;,N#)"/>-->
		<HASMFGDATE COLUMNREFERENCE="N"/>
		<!-- Whether to use Expiry dates or not.  -->
		<!-- If set to Yes, then the Expiry.Date has to be given. -->
		<!--<ISPERISHABLEON FORMULA="=IF(len(trim(AE#))=0,&quot;No&quot;,O#)"/>-->
		<ISPERISHABLEON COLUMNREFERENCE="O"/>
		<!-- Standard Cost details -->
		<STANDARDCOSTLIST.LIST SCROLL="Yes" SKIP="=len(trim(P#))=0" APPEND="No">
			<DATE COLUMNREFERENCE="P"/>
			<RATE FORMULA="=Q# &amp; &quot;/&quot; &amp; +R#"/>
		</STANDARDCOSTLIST.LIST>
		<!-- Standard Price details -->
		<STANDARDPRICELIST.LIST SCROLL="Yes" SKIP="=len(trim(S#))=0" APPEND="No">
			<DATE COLUMNREFERENCE="S"/>
			<RATE FORMULA="=T# &amp; &quot;/&quot; &amp; +U#"/>
		</STANDARDPRICELIST.LIST>
		<!-- Default Sales Ledger Name and VAT Class -->
		<SALESLIST.LIST SCROLL="YES" SKIP="=not(trim(V#)=&quot;sales&quot;)">
			<NAME COLUMNREFERENCE="W"/>
			<VATCLASSIFICATIONNATURE COLUMNREFERENCE="Y"/>
			<TAXCLASSIFICATIONNAME COLUMNREFERENCE="Z"/>
			<CLASSRATE COLUMNREFERENCE="AA"/>
		</SALESLIST.LIST>
		<!-- Default Purchase Ledger Name and VAT Class -->
		<PURCHASELIST.LIST SCROLL="YES" SKIP="=not(trim(V#)=&quot;purchase&quot;)">
			<NAME COLUMNREFERENCE="W"/>
			<VATCLASSIFICATIONNATURE COLUMNREFERENCE="Y"/>
			<TAXCLASSIFICATIONNAME COLUMNREFERENCE="Z"/>
			<CLASSRATE COLUMNREFERENCE="AA"/>
		</PURCHASELIST.LIST>
		<!-- Stock-item Opening Qty  -->
		<OPENINGBALANCE SKIP="=Or({Round(AF#,2)}=0,Not(Round(L#,2)=0))" FORMULA="={Round(AF#,2)}"/>
		<OPENINGBALANCE SKIP="Or({Round(AF#,2)}=0,Round(L#,2)=0)" FORMULA="={AF#} &amp; &quot; &quot; &amp; J# &amp; &quot; = &quot; &amp; Round({AF#}/L#,0) &amp; &quot; &quot; &amp; K#"/>
		<!-- Stock-item Rate -->
		<OPENINGRATE SKIP="={Round(AI#,2)}=0" FORMULA="=IF({Round(AF#,2)}&gt;0,Round({Round(AI#,2)}/{Round(AF#,2)},2) &amp; &quot;/&quot; &amp; AH#,0)"/>
		<!-- Stock-item Value -->
		<OPENINGVALUE SKIP="={Round(AI#,2)}=0" FORMULA="={Round(AI#,2)} * -1"/>
		<!-- Batch wise Opening Stock details details-->
		<BATCHALLOCATIONS.LIST SCROLL="Yes" SKIP="=And(len(trim(AB#))=0,len(trim(AC#))=0)">
			<NAME COLUMNREFERENCE="AC"/>
			<BATCHNAME COLUMNREFERENCE="AC"/>
			<!-- If GodownName is not specified i.e. Column U value is blank, then the Godown Name must be Main Location else value of Column U-->
			<GODOWNNAME FORMULA="=IF(len(trim(AB#))=0,&quot;Main Location&quot;,+AB#)"/>
			<MFDON FORMULA="=IF(len(trim(AD#))=0,&quot;&quot;,AD#)"/>
			<EXPIRYPERIOD FORMULA="=IF(len(trim(AE#))=0,&quot;&quot;,AE#)"/>
			<!-- Stock-item Opening Qty  -->
			<OPENINGBALANCE SKIP="=Or(AF#=0,Not(Round(L#[1],2)=0))" COLUMNREFERENCE="AF"/>
			<OPENINGBALANCE SKIP="Or(AF#=0,Round(L#[1],2)=0)" FORMULA="=AF# &amp; &quot; &quot; &amp; J#[1] &amp; &quot; = &quot; &amp; Round(AF#/L#[1],0) &amp; &quot; &quot; &amp; K#[1]"/>
			<!-- Stock-item Rate -->
			<OPENINGRATE SKIP="=Round(AG#,2)=0" FORMULA="=AG# &amp; &quot;/&quot; &amp; AH#"/>
			<!-- Stock-item Value -->
			<OPENINGVALUE SKIP="=Round(AI#,2)=0" FORMULA="=Round(AI#,2) * -1"/>
		</BATCHALLOCATIONS.LIST>
		<!-- GST details -->
		<GSTAPPLICABLE SKIP="=len(trim(AJ#))=0" FORMULA="=&quot;~[~ &quot; &amp; AJ#"/>
		<!-- Type of supply -->
		<GSTTYPEOFSUPPLY COLUMNREFERENCE="AW"/>
    <!-- Empty GSTDETAILS tag not required as it will overwrite existing data, if any  -->
		<GSTDETAILS.LIST SKIP="=And(len(trim(AL#))=0,len(trim(AM#))=0,len(trim(AW#))=0,len(trim(AR#))=0,len(trim(AU#))=0)">
			<APPLICABLEFROM COLUMNREFERENCE="AK"/>
			<CALCULATIONTYPE COLUMNREFERENCE="AO"/>
			<HSNCODE COLUMNREFERENCE="AL"/>
			<HSNMASTERNAME COLUMNREFERENCE="AX"/>
			<HSN COLUMNREFERENCE="AM"/>
			<TAXABILITY SKIP="=Or(len(trim(AO#[1]))=0,trim(AO#[1])=&quot;On Item Rate&quot;)" COLUMNREFERENCE="AR"/>
			<ISREVERSECHARGEAPPLICABLE COLUMNREFERENCE="AS"/>
			<ISNONGSTGOODS COLUMNREFERENCE="AN"/>
			<GSTINELIGIBLEITC COLUMNREFERENCE="AT"/>
			<STATEWISEDETAILS.LIST>
				<STATENAME>~[~ Any</STATENAME>
				<RATEDETAILS.LIST SKIP="=len(trim(AU#))=0">
					<GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AU#/2,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST SKIP="=len(trim(AU#))=0">
					<GSTRATEDUTYHEAD>State Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AU#/2,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST SKIP="=len(trim(AU#))=0">
					<GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AU#,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST SKIP="=len(trim(AV#))=0">
					<GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AV#,2)"/>
				</RATEDETAILS.LIST>
				<!-- When CALCULATION-TYPE is set as : On Item Rate -->
				<GSTSLABRATES.LIST SKIP="=Or(len(trim(AO#[1]))=0,Not(trim(AO#[1])=&quot;On Item Rate&quot;))" SCROLL="YES">
					<TAXABILITY COLUMNREFERENCE="AR"/>
					<TOITEMRATE FORMULA="=Round(AQ#,2)"/>
					<RATEDETAILS.LIST SKIP="=len(trim(AU#))=0">
						<GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(AU#/2,2)"/>
					</RATEDETAILS.LIST>
					<RATEDETAILS.LIST SKIP="=len(trim(AU#))=0">
						<GSTRATEDUTYHEAD>State Tax</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(AU#/2,2)"/>
					</RATEDETAILS.LIST>
					<RATEDETAILS.LIST SKIP="=len(trim(AU#))=0">
						<GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(AU#,2)"/>
					</RATEDETAILS.LIST>
					<RATEDETAILS.LIST SKIP="=len(trim(AV#))=0">
						<GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
						<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
						<GSTRATE FORMULA="=Round(AV#,2)"/>
					</RATEDETAILS.LIST>
				</GSTSLABRATES.LIST>
			</STATEWISEDETAILS.LIST>
			<TEMPGSTDETAILSLABRATES.LIST>
			</TEMPGSTDETAILSLABRATES.LIST>
		</GSTDETAILS.LIST>
		<!-- VAT Commodity -->
		<VATCOMMODITY COLUMNREFERENCE="AZ"/>
		<!-- Rate of VAT-->
		<RATEOFVAT COLUMNREFERENCE="BE"/>
		<VATBASENO COLUMNREFERENCE="BK"/>
		<VATBASEUNIT COLUMNREFERENCE="BL"/>
		<VATTRAILNO COLUMNREFERENCE="BM"/>
		<VATTRAILUNIT COLUMNREFERENCE="BN"/>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<VATDETAILS.LIST SKIP="=Not(Or(trim(AY#)=&quot;applicable&quot;,trim(AY#)=&quot;yes&quot;))">
			<FROMDATE COLUMNREFERENCE="BG"/>
			<TAXTYPE FORMULA="=if(Len(trim(BH#))=0,&quot;Taxable&quot;,BH#)"/>
			<GOODSNATURE COLUMNREFERENCE="BA"/>
			<VATCOMMODITYNAME COLUMNREFERENCE="BB"/>
			<VATCOMMODITYCODE COLUMNREFERENCE="BC"/>
			<REPORTINGUOM COLUMNREFERENCE="BD"/>
			<ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
			<RATEOFVAT COLUMNREFERENCE="BE"/>
			<ADDITIONALTAX COLUMNREFERENCE="BF"/>
		</VATDETAILS.LIST>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<INCLUSIVETAX COLUMNREFERENCE="BO"/>
		<!-- <VATAPPLICABLE SKIP="=Round(BE#,2)=0">~[~ Applicable</VATAPPLICABLE> -->
		<VATAPPLICABLE SKIP="=len(trim(AY#))=0" FORMULA="=&quot;~[~ &quot; &amp; trim(AY#)"/>
		<!-- <EXCISEAPPLICABILITY SKIP="=And(Round(CF#,2)=0,len(trim(BS#))=0)">~[~ Applicable</EXCISEAPPLICABILITY> -->
		<EXCISEAPPLICABILITY SKIP="=len(trim(BP#))=0" FORMULA="=&quot;~[~ &quot; &amp; trim(BP#)"/>
		<!-- Tariff Classification -->
		<BASICTARIFFTYPE COLUMNREFERENCE="BR"/>
		<!-- Rate of Excise Duty -->
		<BASICRATEOFEXCISE COLUMNREFERENCE="CF"/>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<EXCISETARIFFDETAILS.LIST SKIP="=len(trim(CF#))=0">
			<APPLICABLEFROM COLUMNREFERENCE="CI"/>
			<TYPEOFTARIFF FORMULA="=if(len(trim(BU#))=0,&quot;Dutiable&quot;,BU#)"/>
			<EXCISECLASSIFICATIONNAME COLUMNREFERENCE="BQ"/>
			<STOCKITEMTYPE COLUMNREFERENCE="BW"/>
			<ISEXCISECALCULATEONMRP COLUMNREFERENCE="CB"/>
			<ISNONDUTIABLE>No</ISNONDUTIABLE>
			<ABATEMENTPERCENTAGE COLUMNREFERENCE="CC"/>
			<REPORTINGUOM COLUMNREFERENCE="BT"/>
			<TARIFFNAME COLUMNREFERENCE="BR"/>
			<VALUATIONTYPE FORMULA="=if(len(trim(CE#))=0,&quot;Ad Valorem&quot;,CE#)"/>
			<HSNCODE COLUMNREFERENCE="BS"/>
			<EXCISEDUTYHEADDETAILS.LIST SCROLL="YES">
				<VALUATIONTYPE FORMULA="=if(len(trim(CE#))=0,&quot;Ad Valorem&quot;,CE#)"/>
				<STOCKITEMTYPE COLUMNREFERENCE="BW"/>
				<DUTYHEAD FORMULA="=if(len(trim(CD#))=0,&quot;Basic Excise Duty&quot;,CD#)"/>
				<EXCISERATE COLUMNREFERENCE="CF"/>
				<NOTIFICATIONNUMBER COLUMNREFERENCE="CG"/>
				<NOTIFICATIONSERIAL COLUMNREFERENCE="CH"/>
			</EXCISEDUTYHEADDETAILS.LIST>
		</EXCISETARIFFDETAILS.LIST>
		<EXCISEITEMGODOWN.LIST SKIP="=len(trim(BW#))=0">
			<!-- The TaxUnitName master must exist in Tally  -->
			<TAXUNITNAME COLUMNREFERENCE="BV"/>
			<!-- If StockItemType is specified, then the TaxUnitName must also be specified -->
			<STOCKITEMTYPE COLUMNREFERENCE="BW"/>
			<!-- 
			Ex: 1 Pcs = 32 sq feet / sq meter
			where 
			BASEUNIT =  sq feet; 
			ALTERNATEUNIT = Pcs
			-->
			<!-- 1 PCs -->
			<EXCISEREPDENOMINATOR COLUMNREFERENCE="BX"/>
			<EXCISECONVUNIT COLUMNREFERENCE="BY"/>
			<!-- 32 sq feet -->
			<EXCISEALTREPUNITS COLUMNREFERENCE="BZ"/>
			<EXCISEREPUNITSSTR COLUMNREFERENCE="CA"/>
		</EXCISEITEMGODOWN.LIST>
		<!-- MRP -->
		<RATEOFMRP COLUMNREFERENCE="CJ"/>
		<!-- For Tally ERP 9 Release 5.0 or higher -->
		<MRPDETAILS.LIST SKIP="=len(trim(CJ#))=0">
			<FROMDATE FORMULA="=(&quot;20160401&quot;)"/>
			<MRPRATEDETAILS.LIST>
				<MRPRATE COLUMNREFERENCE="CJ"/>
			</MRPRATEDETAILS.LIST>
		</MRPDETAILS.LIST>
		<!-- Calc Tax on MRP / MRP to print -->
		<CALCONMRP COLUMNREFERENCE="BI"/>
		<!-- MRP Incl of VAT -->
		<ISMRPINCLOFTAX COLUMNREFERENCE="BJ"/>
		<!-- Behaviour -->
		<COSTINGMETHOD COLUMNREFERENCE="CK"/>
		<VALUATIONMETHOD COLUMNREFERENCE="CL"/>
		<IGNOREPHYSICALDIFFERENCE COLUMNREFERENCE="CM"/>
		<IGNORENEGATIVESTOCK COLUMNREFERENCE="CN"/>
		<TREATSALESASMANUFACTURED COLUMNREFERENCE="CO"/>
		<TREATPURCHASESASCONSUMED COLUMNREFERENCE="CP"/>
		<TREATREJECTSASSCRAP COLUMNREFERENCE="CQ"/>
		<!-- REORDER DETAILS -->
		<REORDERBASE COLUMNREFERENCE="CR"/>
		<REORDERPERIODLENGTH COLUMNREFERENCE="CS"/>
		<REORDERPERIOD COLUMNREFERENCE="CT"/>
		<!-- Reorder Level -->
		<REORDERASHIGHER SKIP="=len(trim(CU#))=0" FORMULA="=If(CU#=&quot;Higher&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
		<!-- MINIMUM REORDER DETAILS -->
		<MINIMUMORDERBASE COLUMNREFERENCE="CV"/>
		<MINORDERPERIODLENGTH COLUMNREFERENCE="CW"/>
		<MINORDERPERIOD COLUMNREFERENCE="CX"/>
		<MINORDERASHIGHER SKIP="=len(trim(CY#))=0" FORMULA="=If(CY#=&quot;Higher&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
		<!-- Conversion factor details-->
		<DENOMINATOR FORMULA="=IF(len(trim(K#))=0,&quot;&quot;,L#)"/>
		<CONVERSION FORMULA="=IF(len(trim(K#))=0,&quot;&quot;,1)"/>
	</MASTER>
</XMLTAGS>

