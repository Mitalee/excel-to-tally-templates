<!-- These XML tags are added to Excel sheet at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that this (i.e Column as mentioned above) is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>ID</COLUMNNAME>
		<COLUMNNAME>NAME</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!-- Create LEDGER Masters -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="E"/>
		</NAME.LIST>
		<PARENT>SUNDRY CREDITORS</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
	</MASTER>
	<!-- Create a single LEDGER Master -->
	<MASTER TYPE="LEDGER" SINGLERECORD="Yes">
		<NAME.LIST>
			<NAME>Purchase</NAME>
		</NAME.LIST>
		<PARENT>Purchase Accounts</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK>Yes</AFFECTSSTOCK>
	</MASTER>
	<!-- Create Purchase LEDGER Masters -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="K"/>
		</NAME.LIST>
		<PARENT>Purchase Accounts</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK>Yes</AFFECTSSTOCK>
	</MASTER>
	<!-- Create UNIT Master -->
	<MASTER TYPE="UNIT">
		<NAME COLUMNREFERENCE="G"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create STOCKITEM Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="F"/>
		</NAME.LIST>
		<PARENT/>
		<BASEUNITS COLUMNREFERENCE="G"/>
	</MASTER>
	<VOUCHER>
		<GUID FORMULA="=+&quot;udi-HC6KQ9-&quot; &amp; A# &amp; &quot;-&quot; &amp; B# &amp; &quot;-&quot; &amp; D#"/>
		<!--GUID FORMULA="=+&quot;udi-purc-&quot; &amp; A# &amp; &quot;-&quot; &amp; B# &amp; &quot;-&quot; &amp; D#" /-->
		<DATE COLUMNREFERENCE="D"/>
		<EFFECTIVEDATE COLUMNREFERENCE="D"/>
		<VOUCHERTYPENAME>Purchase</VOUCHERTYPENAME>
		<VOUCHERNUMBER COLUMNREFERENCE="B"/>
		<REFERENCE COLUMNREFERENCE="C"/>

		<NARRATION COLUMNREFERENCE="L"/>
		<ISINVOICE>Yes</ISINVOICE>
		<!-- Party Ledger to be credited -->
		<LEDGERENTRIES.LIST>
			<LEDGERNAME COLUMNREFERENCE="E"/>
			<ISDEEMEDPOSITIVE>No</ISDEEMEDPOSITIVE>
			<AMOUNT FORMULA="={Round(J#,2)}*1"/>
			<BILLALLOCATIONS.LIST SKIP="=Len(trim(B#))=0">
				<NAME COLUMNREFERENCE="B"/>
				<BILLTYPE>New Ref</BILLTYPE>
				<AMOUNT FORMULA="={Round(J#,2)}*1"/>
			</BILLALLOCATIONS.LIST>
		</LEDGERENTRIES.LIST>
		<ALLINVENTORYENTRIES.LIST SCROLL="YES" SKIP="=Len(trim(F#))=0">
			<STOCKITEMNAME COLUMNREFERENCE="F"/>
			<ISDEEMEDPOSITIVE>Yes</ISDEEMEDPOSITIVE>
			<RATE COLUMNREFERENCE="I"/>
			<AMOUNT FORMULA="=Round(J#,2)*-1"/>
			<ACTUALQTY COLUMNREFERENCE="H"/>
			<BILLEDQTY COLUMNREFERENCE="H"/>
			<ACCOUNTINGALLOCATIONS.LIST>
				<LEDGERNAME FORMULA="=If(Len(trim(K#))=0,&quot;Purchase&quot;,K#)"/>
				<ISDEEMEDPOSITIVE>Yes</ISDEEMEDPOSITIVE>
				<AMOUNT FORMULA="=Round(J#,2)*-1"/>
			</ACCOUNTINGALLOCATIONS.LIST>
			<BATCHALLOCATIONS.LIST>
				<GODOWNNAME>Main Location</GODOWNNAME>
				<BATCHNAME>Primary Batch</BATCHNAME>
				<DESTINATIONGODOWNNAME>Main Location</DESTINATIONGODOWNNAME>
				<AMOUNT FORMULA="=Round(J#,2)*-1"/>
				<ACTUALQTY COLUMNREFERENCE="H"/>
				<BILLEDQTY COLUMNREFERENCE="H"/>
			</BATCHALLOCATIONS.LIST>
		</ALLINVENTORYENTRIES.LIST>
	</VOUCHER>
</XMLTAGS>

