<!-- These XML tags are added to Excel sheet at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" FROMROW="3" xmlns:UDF="TallyUDF">
	<!-- Specifies that this (i.e Column as mentioned above) is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>NAME</COLUMNNAME>
		<COLUMNNAME>ID</COLUMNNAME>
	</COLUMNNAME.LIST>
	<MASTER TYPE="GROUP">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="E"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="F"/>
	</MASTER>
	<MASTER TYPE="CURRENCY">
		<ORIGINALNAME COLUMNREFERENCE="K"/>
		<EXPANDEDSYMBOL FORMULA="=K# &amp; &quot; Curr.&quot;"/>
		<MAILINGNAME FORMULA="=K# &amp; &quot; Curr.&quot;"/>
		<ASORIGINAL>Yes</ASORIGINAL>
		<HASSPACE>No</HASSPACE>
		<INMILLIONS>No</INMILLIONS>
		<DECIMALPLACES> 2</DECIMALPLACES>
	</MASTER>
	<MASTER TYPE="LEDGER">
		<NAME.LIST SKIP="=len(trim(D#))=0">
			<NAME COLUMNREFERENCE="B"/>
			<NAME SCROLL="YES" COLUMNREFERENCE="D"/>
		</NAME.LIST>
		<NAME SKIP="=Not(len(trim(D#))=0)" COLUMNREFERENCE="B"/>
		<OLDNAME COLUMNREFERENCE="C"/>
		<ADDITIONALNAME COLUMNREFERENCE="B"/>
		<PARENT COLUMNREFERENCE="E"/>
		<DESCRIPTION COLUMNREFERENCE="G"/>
		<NARRATION COLUMNREFERENCE="H"/>
		<CURRENCYNAME COLUMNREFERENCE="K"/>
		<CREDITLIMIT COLUMNREFERENCE="P"/>
		<ISCREDITDAYSCHKON COLUMNREFERENCE="Q"/>
		<OVERRIDECREDITLIMIT COLUMNREFERENCE="R"/>
		<OPENINGBALANCE SKIP="=And(len(trim(L#))=0,len(trim(M#))=0)" FORMULA="=Round(L#+M#,2)*if(L#&gt;0,-1,1)"/>
		<ISBILLWISEON COLUMNREFERENCE="N"/>
		<BILLCREDITPERIOD COLUMNREFERENCE="O"/>
		<BILLALLOCATIONS.LIST SKIP="=len(trim(S#))=0" SCROLL="YES">
			<NAME COLUMNREFERENCE="S"/>
			<BILLDATE COLUMNREFERENCE="T"/>
			<BILLCREDITPERIOD COLUMNREFERENCE="U"/>
			<OPENINGBALANCE FORMULA="=Round(V#+W#,2)*if(V#&gt;0,-1,1)"/>
		</BILLALLOCATIONS.LIST>
		<ADDRESS.LIST SKIP="=And(len(trim(AF#))=0,len(trim(AG#))=0,len(trim(AH#))=0,len(trim(AI#))=0)">
			<ADDRESS COLUMNREFERENCE="AF"/>
			<ADDRESS COLUMNREFERENCE="AG"/>
			<ADDRESS COLUMNREFERENCE="AH"/>
			<ADDRESS COLUMNREFERENCE="AI"/>
		</ADDRESS.LIST>
		<LEDGERCONTACT COLUMNREFERENCE="X"/>
		<LEDGERPHONE COLUMNREFERENCE="Y"/>
		<LEDGERMOBILE COLUMNREFERENCE="Z"/>
		<LEDGERFAX COLUMNREFERENCE="AA"/>
		<EMAIL COLUMNREFERENCE="AB"/>
		<COUNTRYNAME COLUMNREFERENCE="AC"/>
		<STATENAME COLUMNREFERENCE="AD"/>
		<!--<LEDSTATENAME COLUMNREFERENCE="AD"/>-->
		<PINCODE COLUMNREFERENCE="AE"/>
		<!-- GST fields for PARTY LEDGER -->
		<GSTREGISTRATIONTYPE COLUMNREFERENCE="AJ"/>
		<!-- GSTIN / UIN -->
		<PARTYGSTIN COLUMNREFERENCE="AK"/>
		<!-- Party Type -->
		<GSTNATUREOFSUPPLY COLUMNREFERENCE="AL"/>
		<ISECOMMOPERATOR COLUMNREFERENCE="AM"/>
		<!-- GST fields for SALES LEDGER -->
		<!-- Is GST Applicable -->
		<GSTAPPLICABLE SKIP="=len(trim(AN#))=0" FORMULA="= &quot;~[~ &quot; &amp; AN#"/>
		<!-- Type of supply -->
		<GSTTYPEOFSUPPLY COLUMNREFERENCE="AV"/>
		<!-- Empty GSTDETAILS tag not required as it will overwrite existing data, if any  -->
		<GSTDETAILS.LIST SKIP="=And(len(trim(AO#))=0,len(trim(AP#))=0,len(trim(AQ#))=0,len(trim(AR#))=0,len(trim(AR#))=0,len(trim(AT#))=0)">
			<!-- Applicable date -->
			<APPLICABLEFROM COLUMNREFERENCE="AO"/>
			<!-- HSN/SAC code -->
			<HSNCODE COLUMNREFERENCE="AP"/>
			<!-- HSN description -->
			<HSN COLUMNREFERENCE="AQ"/>
			<!-- Taxability -->
			<TAXABILITY COLUMNREFERENCE="AS"/>
			<!-- Nature of Transaction -->
			<GSTNATUREOFTRANSACTION COLUMNREFERENCE="AR"/>
			<ISREVERSECHARGEAPPLICABLE>No</ISREVERSECHARGEAPPLICABLE>
			<ISNONGSTGOODS>No</ISNONGSTGOODS>
			<GSTINELIGIBLEITC>No</GSTINELIGIBLEITC>
			<STATEWISEDETAILS.LIST>
				<STATENAME>~[~ Any</STATENAME>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AT#/2,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>State Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AT#/2,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AT#,2)"/>
				</RATEDETAILS.LIST>
				<RATEDETAILS.LIST>
					<GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
					<GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
					<GSTRATE FORMULA="=Round(AU#,2)"/>
				</RATEDETAILS.LIST>
				<GSTSLABRATES.LIST>
				</GSTSLABRATES.LIST>
			</STATEWISEDETAILS.LIST>
			<TEMPGSTDETAILSLABRATES.LIST>
			</TEMPGSTDETAILSLABRATES.LIST>
		</GSTDETAILS.LIST>
		<!-- GST fields for DUTIES AND TAXES LEDGER -->
		<!-- This tag appears for CGST only -->
		<GSTAPPLICABLE SKIP="=Or(len(trim(AX#))=0,Not(trim(AX#)=&quot;central tax&quot;))">~[~ Applicable</GSTAPPLICABLE>
		<!-- GST -->
		<TAXTYPE COLUMNREFERENCE="AW"/>
		<!-- Central Tax or State Tax or Integrated Tax -->
		<GSTDUTYHEAD COLUMNREFERENCE="AX"/>
		<!-- GST Tax Rate -->
		<RATEOFTAXCALCULATION COLUMNREFERENCE="AY"/>
		<!-- Always blank -->
		<GSTTYPE/>
		<!-- FOR VAT -->
		<TAXTYPE COLUMNREFERENCE="AZ"/>
		<!-- If VAT Rate is specified and is greater than 0, then set the tag VATAPPLICABLE -->
		<VATAPPLICABLE SKIP="=Round(BB#,2)=0">~[~ Applicable</VATAPPLICABLE>
		<RATEOFTAXCALCULATION COLUMNREFERENCE="BB"/>
		<VATDETAILS.LIST SKIP="=len(trim(BA#))=0">
			<FROMDATE COLUMNREFERENCE="BE"/>
			<TAXTYPE COLUMNREFERENCE="BD"/>
			<STATNATURENAME COLUMNREFERENCE="BA"/>
			<ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
			<ISCALCONACTUALQTY>No</ISCALCONACTUALQTY>
			<RATEOFVAT COLUMNREFERENCE="BB"/>
			<ADDITIONALTAX COLUMNREFERENCE="BC"/>
		</VATDETAILS.LIST>
		<INCOMETAXNUMBER COLUMNREFERENCE="BF"/>
		<REGISTRATIONNUMBER COLUMNREFERENCE="BG"/>
		<VATDEALERTYPE COLUMNREFERENCE="BH"/>
		<VATTINNUMBER COLUMNREFERENCE="BI"/>
		<SALESTAXNUMBER COLUMNREFERENCE="BI"/>
		<UDF:VATREGISTRATIONDATE.LIST DESC="`VATRegistrationDate`" ISLIST="YES" TYPE="Date" SKIP="=len(trim(BJ#))=0">
			<UDF:VATREGISTRATIONDATE DESC="`VATRegistrationDate`" COLUMNREFERENCE="BJ"/>
		</UDF:VATREGISTRATIONDATE.LIST>
		<INTERSTATESTNUMBER COLUMNREFERENCE="BK"/>
		<UDF:CSTREGISTRATIONDATE.LIST DESC="`CSTRegistrationDate`" ISLIST="YES" TYPE="Date" SKIP="=len(trim(BL#))=0">
			<UDF:CSTREGISTRATIONDATE DESC="`CSTRegistrationDate`" COLUMNREFERENCE="BL"/>
		</UDF:CSTREGISTRATIONDATE.LIST>
		<ISTDSAPPLICABLE COLUMNREFERENCE="BV"/>
		<ISSTXPARTY COLUMNREFERENCE="BW"/>
		<PAYMENTDETAILS.LIST SKIP="=And(len(trim(BR#))=0,len(trim(BS#))=0,len(trim(BT#))=0)">
			<IFSCODE COLUMNREFERENCE="BR"/>
			<BANKNAME COLUMNREFERENCE="BT"/>
			<ACCOUNTNUMBER COLUMNREFERENCE="BS"/>
			<PAYMENTFAVOURING COLUMNREFERENCE="B"/>
			<TRANSACTIONNAME COLUMNREFERENCE="B"/>
			<SETASDEFAULT>Yes</SETASDEFAULT>
			<DEFAULTTRANSACTIONTYPE>Inter Bank Transfer</DEFAULTTRANSACTIONTYPE>
		</PAYMENTDETAILS.LIST>
		<AFFECTSSTOCK COLUMNREFERENCE="I"/>
		<EXCISEREGNO COLUMNREFERENCE="BM"/>
		<EXCISEREGISTRATIONDATE COLUMNREFERENCE="BN"/>
		<EXCISERANGE COLUMNREFERENCE="BO"/>
		<EXCISEDIVISION COLUMNREFERENCE="BP"/>
		<EXCISECOMMISSIONERATE COLUMNREFERENCE="BQ"/>
		<ISCOSTCENTRESON COLUMNREFERENCE="J"/>
		<PRICELEVEL COLUMNREFERENCE="BU"/>
	</MASTER>
</XMLTAGS>

