<!-- These XML tags are added to Excel sheet specified Cell at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">

	<!-- Specifies that the Cell as mentioned above is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>ID</COLUMNNAME>
		<COLUMNNAME>NAME</COLUMNNAME>
	</COLUMNNAME.LIST>

	<!-- Create a Single VOUCHERTYPE Master record -->
	<MASTER TYPE="VOUCHERTYPE" SINGLERECORD="Yes">
		<NAME.LIST TYPE="String">
			<NAME>Production</NAME>
		</NAME.LIST>
		<PARENT>Stock Journal</PARENT>
		<MAILINGNAME>Stk Jrnl</MAILINGNAME>
		<NUMBERINGMETHOD>Automatic</NUMBERINGMETHOD>
		<ISDEEMEDPOSITIVE>Yes</ISDEEMEDPOSITIVE>
		<AFFECTSSTOCK>Yes</AFFECTSSTOCK>
		<PREVENTDUPLICATES>No</PREVENTDUPLICATES>
		<PREFILLZERO>No</PREFILLZERO>
	</MASTER>

	<!-- Create UNIT Master -->
	<MASTER TYPE="UNIT" >
		<NAME COLUMNREFERENCE="D"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<MASTER TYPE="UNIT">
		<NAME COLUMNREFERENCE="J"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>

	<!-- Create Godown Masters -->
	<MASTER TYPE="GODOWN">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="E"/>
		</NAME.LIST>
		<PARENT/>
	</MASTER>

	<!-- Create Godown Masters -->
	<MASTER TYPE="GODOWN">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="K"/>
		</NAME.LIST>
		<PARENT/>
	</MASTER>

	<!-- Create StockItem Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="C"/>
		</NAME.LIST>
		<PARENT/>
		<COSTINGMETHOD>Avg. Cost</COSTINGMETHOD>
		<VALUATIONMETHOD>Avg. Price</VALUATIONMETHOD>
		<BASEUNITS COLUMNREFERENCE="D"/>
	</MASTER>

	<!-- Create StockItem Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="I"/>
		</NAME.LIST>
		<PARENT/>
		<COSTINGMETHOD>Avg. Cost</COSTINGMETHOD>
		<VALUATIONMETHOD>Avg. Price</VALUATIONMETHOD>
		<BASEUNITS COLUMNREFERENCE="J"/>
	</MASTER>

	<!-- XML tags for creating Vouchers -->
	<VOUCHER>
		<GUID FORMULA="=&quot;udi-G94MRN-&quot; &amp; A# &amp; &quot;-&quot; &amp; B#"/>
		<!--GUID FORMULA="=&quot;udi-stock-jrnl-&quot; &amp; A# &amp; &quot;-&quot; &amp; B#"/-->

		<DATE COLUMNREFERENCE="B"/>
		<EFFECTIVEDATE COLUMNREFERENCE="B"/>
		<VOUCHERTYPENAME>Production</VOUCHERTYPENAME>
		<ISINVOICE>Yes</ISINVOICE>
		<VOUCHERNUMBER COLUMNREFERENCE="A"/>
		<NARRATION COLUMNREFERENCE="O"/>

		<!-- StockIn-->
		<INVENTORYENTRIESIN.LIST SCROLL="Yes">
			<STOCKITEMNAME  COLUMNREFERENCE="I"/>
			<ISDEEMEDPOSITIVE>Yes</ISDEEMEDPOSITIVE>
			<RATE FORMULA="=Round(M#,2)"/>
			<AMOUNT  FORMULA="=Round(N#,2)*-1"/>
			<ACTUALQTY FORMULA="=Round(L#,2)"/>
			<BILLEDQTY FORMULA="=Round(L#,2)"/>
			<BATCHALLOCATIONS.LIST>
				<GODOWNNAME FORMULA="=if(len(trim(K#))=0,&quot;Main Location&quot;,K#)"/>
				<BATCHNAME>Primary Batch</BATCHNAME>
				<DESTINATIONGODOWNNAME COLUMNREFERENCE="K"/>
				<AMOUNT FORMULA="=Round(N#,2)*-1"/>
				<ACTUALQTY  FORMULA="=Round(L#,2)"/>
				<BILLEDQTY  FORMULA="=Round(L#,2)"/>
			</BATCHALLOCATIONS.LIST>
		</INVENTORYENTRIESIN.LIST>

		<INVENTORYENTRIESOUT.LIST SCROLL="YES">
			<STOCKITEMNAME COLUMNREFERENCE="C"/>
			<ISDEEMEDPOSITIVE>No</ISDEEMEDPOSITIVE>
			<RATE  FORMULA="=Round(G#,2)"/>
			<AMOUNT FORMULA="=Round(H#,2)"/>
			<ACTUALQTY FORMULA="=Round(F#,2)"/>
			<BILLEDQTY FORMULA="=Round(F#,2)"/>
			<BATCHALLOCATIONS.LIST>
				<GODOWNNAME FORMULA="=if(len(trim(E#))=0,&quot;Main Location&quot;,E#)"/>
				<BATCHNAME>Primary Batch</BATCHNAME>
				<AMOUNT  FORMULA="=Round(H#,2)"/>
				<ACTUALQTY FORMULA="=Round(F#,2)"/>
				<BILLEDQTY FORMULA="=Round(F#,2)"/>
			</BATCHALLOCATIONS.LIST>
		</INVENTORYENTRIESOUT.LIST>

	</VOUCHER>
</XMLTAGS>
