<!-- These XML tags are added to Excel sheet specified Cell at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that the Cell as mentioned above is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>ID</COLUMNNAME>
		<COLUMNNAME>NAME</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!-- Create a Single VOUCHERTYPE Master record -->
	<MASTER TYPE="VOUCHERTYPE" SINGLERECORD="Yes">
		<NAME.LIST TYPE="String">
			<NAME COLUMNREFERENCE="C"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="B"/>
		<MAILINGNAME>Stk Jrnl</MAILINGNAME>
		<NUMBERINGMETHOD>Automatic</NUMBERINGMETHOD>
		<ISDEEMEDPOSITIVE>Yes</ISDEEMEDPOSITIVE>
		<AFFECTSSTOCK>Yes</AFFECTSSTOCK>
		<PREVENTDUPLICATES>No</PREVENTDUPLICATES>
		<PREFILLZERO>No</PREFILLZERO>
	</MASTER>
	<!-- Create UNIT Master -->
	<MASTER TYPE="UNIT">
		<NAME COLUMNREFERENCE="G"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<MASTER TYPE="UNIT">
		<NAME COLUMNREFERENCE="M"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create Godown Masters -->
	<MASTER TYPE="GODOWN">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="H"/>
		</NAME.LIST>
		<PARENT/>
	</MASTER>
	<!-- Create Godown Masters -->
	<MASTER TYPE="GODOWN">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="N"/>
		</NAME.LIST>
		<PARENT/>
	</MASTER>
	<!-- Create StockItem Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="F"/>
		</NAME.LIST>
		<PARENT/>
		<COSTINGMETHOD>Avg. Cost</COSTINGMETHOD>
		<VALUATIONMETHOD>Avg. Price</VALUATIONMETHOD>
		<BASEUNITS COLUMNREFERENCE="G"/>
	</MASTER>
	<!-- Create StockItem Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="L"/>
		</NAME.LIST>
		<PARENT/>
		<COSTINGMETHOD>Avg. Cost</COSTINGMETHOD>
		<VALUATIONMETHOD>Avg. Price</VALUATIONMETHOD>
		<BASEUNITS COLUMNREFERENCE="M"/>
	</MASTER>
	<!-- XML tags for creating Vouchers -->
	<VOUCHER>
		<GUID FORMULA="=&quot;udi-G94MRN-&quot; &amp; A# &amp; &quot;-&quot; &amp; E#"/>
		<!--GUID FORMULA="=&quot;udi-stock-jrnl-&quot; &amp; A# &amp; &quot;-&quot; &amp; E#"/-->
		<DATE COLUMNREFERENCE="E"/>
		<EFFECTIVEDATE COLUMNREFERENCE="E"/>
		<VOUCHERTYPENAME COLUMNREFERENCE="C"/>
		<ISINVOICE>Yes</ISINVOICE>
		<VOUCHERNUMBER COLUMNREFERENCE="A"/>
		<NARRATION COLUMNREFERENCE="R"/>
		<!-- StockIn-->
		<INVENTORYENTRIESIN.LIST SCROLL="Yes">
			<STOCKITEMNAME COLUMNREFERENCE="L"/>
			<ISDEEMEDPOSITIVE>Yes</ISDEEMEDPOSITIVE>
			<RATE FORMULA="=Round(P#,2)"/>
			<AMOUNT FORMULA="=Round(Q#,2)*-1"/>
			<ACTUALQTY FORMULA="=Round(O#,2)"/>
			<BILLEDQTY FORMULA="=Round(O#,2)"/>
			<BATCHALLOCATIONS.LIST>
				<GODOWNNAME FORMULA="=if(len(trim(N#))=0,&quot;Main Location&quot;,N#)"/>
				<BATCHNAME>Primary Batch</BATCHNAME>
				<DESTINATIONGODOWNNAME COLUMNREFERENCE="N"/>
				<AMOUNT FORMULA="=Round(Q#,2)*-1"/>
				<ACTUALQTY FORMULA="=Round(O#,2)"/>
				<BILLEDQTY FORMULA="=Round(O#,2)"/>
			</BATCHALLOCATIONS.LIST>
		</INVENTORYENTRIESIN.LIST>
		<INVENTORYENTRIESOUT.LIST SCROLL="YES">
			<STOCKITEMNAME COLUMNREFERENCE="F"/>
			<ISDEEMEDPOSITIVE>No</ISDEEMEDPOSITIVE>
			<RATE FORMULA="=Round(J#,2)"/>
			<AMOUNT FORMULA="=Round(K#,2)"/>
			<ACTUALQTY FORMULA="=Round(I#,2)"/>
			<BILLEDQTY FORMULA="=Round(I#,2)"/>
			<BATCHALLOCATIONS.LIST>
				<GODOWNNAME FORMULA="=if(len(trim(H#))=0,&quot;Main Location&quot;,H#)"/>
				<BATCHNAME>Primary Batch</BATCHNAME>
				<AMOUNT FORMULA="=Round(K#,2)"/>
				<ACTUALQTY FORMULA="=Round(I#,2)"/>
				<BILLEDQTY FORMULA="=Round(I#,2)"/>
			</BATCHALLOCATIONS.LIST>
		</INVENTORYENTRIESOUT.LIST>
	</VOUCHER>
</XMLTAGS>

