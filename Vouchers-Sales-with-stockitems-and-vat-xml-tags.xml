<!-- These XML tags are added to Excel sheet at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that this (i.e Column as mentioned above) is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>ID</COLUMNNAME>
		<COLUMNNAME>NAME</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!-- Settings file which contains Sales Ledger and other TAX details -->
	<DATA LOADFILE="Vouchers-Sales-with-StockItems-and-VAT-settings.csv"/>
	<!-- Create VOUCHERTYPE Master -->
	<MASTER TYPE="VOUCHERTYPE">
		<NAME.LIST TYPE="String">
			<NAME COLUMNREFERENCE="D"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="C"/>
		<MAILINGNAME>Sale</MAILINGNAME>
		<NUMBERINGMETHOD>Manual</NUMBERINGMETHOD>
		<EXCISEUNITNAME/>
		<ISDEEMEDPOSITIVE>Yes</ISDEEMEDPOSITIVE>
	</MASTER>
	<!-- Create  Sales Ledger Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALELEDGER&quot;)__&quot;"/>
		</NAME.LIST>
		<PARENT>Sales Accounts</PARENT>
		<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATCLASS&quot;)__&quot;"/>
		<TAXTYPE>Others</TAXTYPE>
		<ISBILLWISEON>No</ISBILLWISEON>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK>Yes</AFFECTSSTOCK>
		<USEFORVAT>Yes</USEFORVAT>
		<VATAPPLICABLE>Applicable</VATAPPLICABLE>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<VATDETAILS.LIST>
			<FROMDATE>20150401</FROMDATE>
			<TAXTYPE>Taxable</TAXTYPE>
			<ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
			<ISCALCONACTUALQTY>No</ISCALCONACTUALQTY>
			<RATEOFVAT FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATRATE&quot;)__&quot;"/>
			<ADDITIONALTAX FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATRATE&quot;)__&quot;"/>
		    <VATCOMMODITYNAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATRATE&quot;)__&quot;"/>
		    <VATCOMMODITYCODE FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATRATE&quot;)__&quot;"/>
		</VATDETAILS.LIST>
	</MASTER>
	<!-- Create TAX LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATLEDGER&quot;)__&quot;"/>
		</NAME.LIST>
		<PARENT>Duties &amp; Taxes</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATCLASS&quot;)__&quot;"/>
		<TAXTYPE FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALETYPE&quot;)__&quot;"/>
		<BASICTYPEOFDUTY FORMULA="=if(&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALETYPE&quot;)__&quot;=&quot;CST&quot;,&quot;Sales Tax&quot;,&quot;On VAT Rate&quot;)"/>
		<SUBTAXTYPE FORMULA="=if(&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALETYPE&quot;)__&quot;=&quot;CST&quot;,&quot;&quot;,&quot;Output VAT&quot;)"/>
		<RATEOFTAXCALCULATION FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATRATE&quot;)__&quot;"/>
	</MASTER>
	<!-- Create ADDl. TAX LEDGER Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATLEDGER&quot;)__&quot;"/>
		</NAME.LIST>
		<PARENT>Duties &amp; Taxes</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<TAXTYPE FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALETYPE&quot;)__&quot;"/>
		<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATCLASS&quot;)__&quot;"/>
		<BASICTYPEOFDUTY FORMULA="=if(&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALETYPE&quot;)__&quot;=&quot;CST&quot;,&quot;Sales Tax&quot;,&quot;Additional Tax&quot;)"/>
		<SUBTAXTYPE FORMULA="=if(&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALETYPE&quot;)__&quot;=&quot;CST&quot;,&quot;&quot;,&quot;Output VAT&quot;)"/>
		<RATEOFTAXCALCULATION FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATRATE&quot;)__&quot;"/>
	</MASTER>
	<!-- Create LEDGER Masters -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="E"/>
		</NAME.LIST>
		<PARENT>SUNDRY DEBTORS</PARENT>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<COUNTRYNAME SKIP="=len(trim(F#))=0">India</COUNTRYNAME>
		<LEDSTATENAME COLUMNREFERENCE="F"/>
	</MASTER>
	<!-- Create UNIT Masters -->
	<MASTER TYPE="UNIT">
		<NAME COLUMNREFERENCE="H"/>
		<ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
	</MASTER>
	<!-- Create STOCKITEM Masters -->
	<MASTER TYPE="STOCKITEM">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="G"/>
		</NAME.LIST>
		<PARENT/>
		<BASEUNITS COLUMNREFERENCE="H"/>
	</MASTER>
	<VOUCHER>
		<GUID FORMULA="=+&quot;udi-HVL95Q-&quot; &amp; A# &amp; &quot;-&quot; &amp; B#"/>
		<DATE COLUMNREFERENCE="B"/>
		<EFFECTIVEDATE COLUMNREFERENCE="B"/>
		<VOUCHERTYPENAME COLUMNREFERENCE="D"/>
		<REFERENCE COLUMNREFERENCE="A"/>
		<NARRATION COLUMNREFERENCE="Q"/>
		<ISINVOICE>Yes</ISINVOICE>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<STATENAME COLUMNREFERENCE="F"/>
		
		<!-- Party Ledger to be debited -->
		<LEDGERENTRIES.LIST>
			<LEDGERNAME COLUMNREFERENCE="E"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(C#[1]))=&quot;credit note&quot;,&quot;No&quot;,&quot;Yes&quot;)"/>
			<AMOUNT FORMULA="={Round(K#+O#+P#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,1,-1)"/>
			<BILLALLOCATIONS.LIST>
				<NAME COLUMNREFERENCE="A"/>
				<BILLTYPE>New Ref</BILLTYPE>
				<AMOUNT FORMULA="={Round(K#+O#+P#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,1,-1)"/>
			</BILLALLOCATIONS.LIST>
		</LEDGERENTRIES.LIST>
		<!-- VAT / CST Ledger -->
		<LEDGERENTRIES.LIST GROUPBY="=trim((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;))" SKIP="=Round(O#,2)=0">
			<BASICRATEOFINVOICETAX.LIST TYPE="Number">
				<BASICRATEOFINVOICETAX FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATRATE&quot;)__&quot;"/>
			</BASICRATEOFINVOICETAX.LIST>
			<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATCLASS&quot;)__&quot;"/>
			<LEDGERNAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATLEDGER&quot;)__&quot;"/>
			<RATEOFADDLVAT FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATRATE&quot;)__&quot;"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(C#[1]))=&quot;credit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
			<VATASSESSABLEVALUE FORMULA="={Round(K#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
			<AMOUNT FORMULA="={Round(O#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
			<UDF:ADDNLVATCLASSAMT.LIST DESC="`AddnlVATClassAmt`" ISLIST="YES" TYPE="Amount" ISGROUPED="YES">
				<UDF:ADDNLVATCLASSAMT DESC="`AddnlVATClassAmt`" FORMULA="={Round(K#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
			</UDF:ADDNLVATCLASSAMT.LIST>
			<TAXOBJECTALLOCATIONS.LIST>
				<CATEGORY FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATCLASS&quot;)__&quot;"/>
				<TAXTYPE FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALETYPE&quot;)__&quot;"/>
				<TAXNAME FORMULA="=trim(A#[1])"/>
				<PARTYLEDGER FORMULA="=trim(E#[1])"/>
				<REFTYPE>New Ref</REFTYPE>
				<ISOPTIONAL>No</ISOPTIONAL>
				<ISPANVALID>No</ISPANVALID>
				<ZERORATED>No</ZERORATED>
				<SUBCATEGORYALLOCATION.LIST SKIP="=len(trim(G#))=0" SCROLL="Yes">
					<STOCKITEMNAME COLUMNREFERENCE="G"/>
					<SUBCATEGORY FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALETYPE&quot;)__&quot;"/>
					<DUTYLEDGER FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATLEDGER&quot;)__&quot;"/>
					<SUBCATZERORATED>No</SUBCATZERORATED>
					<SUBCATEXEMPTED>No</SUBCATEXEMPTED>
					<SUBCATISSPECIALRATE>No</SUBCATISSPECIALRATE>
					<TAXRATE FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATRATE&quot;)__&quot;"/>
					<ASSESSABLEAMOUNT FORMULA="=Round(K#,2)*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
					<TAX FORMULA="=Round(O#,2)*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
					<BILLEDQTY COLUMNREFERENCE="I"/>
				</SUBCATEGORYALLOCATION.LIST>
			</TAXOBJECTALLOCATIONS.LIST>
			<UDF:MAHWCASSESSABLEVALUE.LIST DESC="`MAHWCAssessableValue`" ISLIST="YES" TYPE="Amount">
				<UDF:MAHWCASSESSABLEVALUE DESC="`MAHWCAssessableValue`" FORMULA="={Round(K#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
			</UDF:MAHWCASSESSABLEVALUE.LIST>
		</LEDGERENTRIES.LIST>
		<!-- Additional Tax-->
		<LEDGERENTRIES.LIST GROUPBY="=trim((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;))" SKIP="=Round(P#,2)=0">
			<BASICRATEOFINVOICETAX.LIST TYPE="Number">
				<BASICRATEOFINVOICETAX FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATRATE&quot;)__&quot;"/>
			</BASICRATEOFINVOICETAX.LIST>
			<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATCLASS&quot;)__&quot;"/>
			<LEDGERNAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATLEDGER&quot;)__&quot;"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(C#[1]))=&quot;credit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
			<AMOUNT FORMULA="={Round(P#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
			<VATASSESSABLEVALUE FORMULA="={Round(K#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
			<TAXOBJECTALLOCATIONS.LIST>
				<CATEGORY FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATCLASS&quot;)__&quot;"/>
				<TAXTYPE FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALETYPE&quot;)__&quot;"/>
				<TAXNAME FORMULA="=trim(A#[1])"/>
				<PARTYLEDGER FORMULA="=trim(E#[1])"/>
				<REFTYPE>New Ref</REFTYPE>
				<ISOPTIONAL>No</ISOPTIONAL>
				<ISPANVALID>No</ISPANVALID>
				<SUBCATEGORYALLOCATION.LIST SCROLL="Yes">
					<STOCKITEMNAME COLUMNREFERENCE="G"/>
					<SUBCATEGORY FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALETYPE&quot;)__&quot;"/>
					<DUTYLEDGER FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATLEDGER&quot;)__&quot;"/>
					<TAXRATE FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-ADDLVATRATE&quot;)__&quot;"/>
					<SUBCATZERORATED>No</SUBCATZERORATED>
					<SUBCATEXEMPTED>No</SUBCATEXEMPTED>
					<SUBCATISSPECIALRATE>No</SUBCATISSPECIALRATE>
					<ASSESSABLEAMOUNT FORMULA="=Round(K#,2)*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
					<TAX FORMULA="=Round(P#,2)*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
					<BILLEDQTY COLUMNREFERENCE="I"/>
				</SUBCATEGORYALLOCATION.LIST>
			</TAXOBJECTALLOCATIONS.LIST>
			<UDF:MAHWCASSESSABLEVALUE.LIST DESC="`MAHWCAssessableValue`" ISLIST="YES" TYPE="Amount">
				<UDF:MAHWCASSESSABLEVALUE DESC="`MAHWCAssessableValue`" FORMULA="={Round(K#,2)}*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
			</UDF:MAHWCASSESSABLEVALUE.LIST>
		</LEDGERENTRIES.LIST>
		<!-- StockItems -->
		<ALLINVENTORYENTRIES.LIST SCROLL="YES">
			<STOCKITEMNAME COLUMNREFERENCE="G"/>
			<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(C#[1]))=&quot;credit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
			<RATE COLUMNREFERENCE="J"/>
			<AMOUNT FORMULA="=Round(K#,2)*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
			<ACTUALQTY COLUMNREFERENCE="I"/>
			<BILLEDQTY COLUMNREFERENCE="I"/>
			<ACCOUNTINGALLOCATIONS.LIST>
				<TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-VATCLASS&quot;)__&quot;"/>
				<LEDGERNAME FORMULA="=&quot;__LOOKUP__((L# &amp; &quot; &quot; &amp; M# &amp; &quot;%&quot;) &amp; &quot;-SALELEDGER&quot;)__&quot;"/>
				<ISDEEMEDPOSITIVE FORMULA="=if(lower(trim(C#[1]))=&quot;credit note&quot;,&quot;Yes&quot;,&quot;No&quot;)"/>
				<AMOUNT FORMULA="=Round(K#,2)*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
			</ACCOUNTINGALLOCATIONS.LIST>
			<BATCHALLOCATIONS.LIST>
				<GODOWNNAME>Main Location</GODOWNNAME>
				<BATCHNAME>Primary Batch</BATCHNAME>
				<DESTINATIONGODOWNNAME>Main Location</DESTINATIONGODOWNNAME>
				<AMOUNT FORMULA="=Round(K#,2)*if(lower(trim(C#[1]))=&quot;credit note&quot;,-1,1)"/>
				<ACTUALQTY COLUMNREFERENCE="I"/>
				<BILLEDQTY COLUMNREFERENCE="I"/>
			</BATCHALLOCATIONS.LIST>
		</ALLINVENTORYENTRIES.LIST>
	</VOUCHER>
</XMLTAGS>

